local function AGUP_fake_script() -- TextButton_9.LocalScript 
	local script = Instance.new('LocalScript', TextButton_9)

	local autoshoot = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if autoshoot == false then
			autoshoot = true
			script.Parent.Text = "STOP SHOOT ALL SCPS"
			repeat
				task.wait(.1)
				for _,scps in pairs(workspace.scps:GetChildren()) do
					local Head = scps:FindFirstChild("Head")
					local Hrp = scps:FindFirstChild("HumanoidRootPart")
					if Head then
						local args = {
							Hrp.CFrame,
						    Head.CFrame
						}
						game:GetService("ReplicatedStorage"):WaitForChild("remotes"):WaitForChild("shoot"):FireServer(unpack(args))
					end
				end
			until autoshoot == false
		else
			script.Parent.Text = "SHOOT ALL SCPS"
			autoshoot = false
		end
	end)
end

coroutine.wrap(PJSQRI_fake_script)()
local function MNZNVUP_fake_script() -- TextButton_11.LocalScript 
	local script = Instance.new('LocalScript', TextButton_11)

	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local LocalPlayer = Players.LocalPlayer
	local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local Hrp = Character:WaitForChild("HumanoidRootPart")
	local TextBox = script.Parent.TextBox
	local Button = script.Parent
	
	local isActive = false
	local loopDelay = .1
	
	-- Find nearest animal within max distance
	local function getNearestAnimal(maxDistance)
		local nearest = nil
		local nearestDist = maxDistance
		for _, animal in pairs(workspace:WaitForChild("animals"):GetChildren()) do
			local animalHrp = animal:FindFirstChild("HumanoidRootPart")
			local mainPart = animal:FindFirstChild("main")
			if animalHrp and mainPart then
				local dist = (Hrp.Position - animalHrp.Position).Magnitude
				if dist <= nearestDist then
					nearest = animal
					nearestDist = dist
				end
			end
		end
		return nearest
	end
	
	-- Function to start shooting loop
	local function startShooting()
		spawn(function()
			while isActive do
				local maxDist = tonumber(TextBox.Text)
				if maxDist then
					local nearestAnimal = getNearestAnimal(maxDist)
					if nearestAnimal and nearestAnimal:FindFirstChild("HumanoidRootPart") and nearestAnimal:FindFirstChild("main") then
						local args = {
							nearestAnimal.HumanoidRootPart.CFrame,
							nearestAnimal.main.CFrame
						}
						ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot"):FireServer(unpack(args))
					end
				else
					warn("Enter a valid number for max distance!")
				end
				task.wait(loopDelay)
			end
		end)
	end
	
	-- Toggle button
	Button.MouseButton1Click:Connect(function()
		isActive = not isActive
		if isActive then
			startShooting()
		end
	end)
end
coroutine.wrap(MNZNVUP_fake_script)()
local function ZAOL_fake_script() -- TextButton_12.LocalScript 
	local script = Instance.new('LocalScript', TextButton_12)

	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local LocalPlayer = Players.LocalPlayer

local function SONKRCM_fake_script() -- ButtonBar.LocalScript 
	local script = Instance.new('LocalScript', ButtonBar)

	local function hideFrames()
		for _,frame in pairs(script.Parent.Parent:GetChildren()) do
			if frame:IsA("Frame") then
				if frame.Name == "ButtonBar" then
				else
					frame.Visible = false
				end
			end
		end
	end
	
	local function ShowSpecificFrame(frameName)
		local VValue = script.Parent.Parent:FindFirstChild("FrameV")
		local frame = script.Parent.Parent:FindFirstChild(frameName)
		if frame then
			VValue.Value = frameName
			hideFrames()
			frame.Visible = true
		end
	end
	
	script.Parent.auto.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.AutoMenu.Name)
	end)
	
	script.Parent.esp.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.EspMenu.Name)
	end)
	
	script.Parent.fish.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.FishMenu.Name)
	end)
	
	script.Parent.kill.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.KillMenu.Name)
	end)
	
	script.Parent.menu.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.WelcomeMenu.Name)
	end)
	
	script.Parent.misc.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.MiscMenu.Name)
	end)
	
	script.Parent.build.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.BuildMenu.Name)
	end)
	
	script.Parent.farm.MouseButton1Click:Connect(function()
		ShowSpecificFrame(script.Parent.Parent.FarmMenu.Name)
	end)
end
coroutine.wrap(SONKRCM_fake_script)()
local function OGYT_fake_script() -- Bar.UIDrag 
	local script = Instance.new('LocalScript', Bar)

	-- Made by Real_IceyDev (@lceyDex) --
	-- Simple UI dragger (PC Only/Any device that has a mouse) --
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(OGYT_fake_script)()
local function NNLLQ_fake_script() -- TextButton_37.LocalScript 
	local script = Instance.new('LocalScript', TextButton_37)

	script.Parent.MouseButton1Click:Connect(function()
		for _,Builds in pairs(workspace:FindFirstChild("builds"):GetChildren()) do
			local args = {
				Builds
			}
			game:GetService("ReplicatedStorage"):WaitForChild("remotes"):WaitForChild("delete"):FireServer(unpack(args))
		end
	end)
end
coroutine.wrap(NNLLQ_fake_script)()
local function UUMQM_fake_script() -- TextBox_5.LocalScript 
	local script = Instance.new('LocalScript', TextBox_5)

	local button = script.Parent.Circle
	local RepStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local deleteRemote = RepStorage:WaitForChild("remotes"):WaitForChild("delete")
	local buildRemote = RepStorage:WaitForChild("remotes"):WaitForChild("build")
	
	-- Helper: find player by name
	local function getTarget(name)
		for _, p in pairs(Players:GetPlayers()) do
			if p.Name:lower():find(name:lower()) 
				or p.DisplayName:lower():find(name:lower())
				or p.DisplayName:sub(1, #name):lower() == name:lower() then
				return p.Character
			end
		end
		return nil
	end
	
	button.MouseButton1Click:Connect(function()
		local targetBox = script.Parent
		local target = getTarget(targetBox.Text)
		if not target or not target:FindFirstChild("HumanoidRootPart") then
			warn("Target not found or missing HumanoidRootPart")
			return
		end
	
		local targetHRP = target.HumanoidRootPart
		local centerPos = targetHRP.Position
		local buildsFolder = workspace:WaitForChild("builds")
	
		local walls = {}
		for _, wall in pairs(buildsFolder:GetChildren()) do
			if wall.Name == "Wall" then
				table.insert(walls, wall)
			end
		end
	
		local wallCount = #walls
		if wallCount == 0 then
			warn("No walls found in builds folder")
			return
		end
	
		-- Precompute offsets for circle
		local radius = 10
		local offsets = {}
		for i = 1, wallCount do
			local angle = (2 * math.pi / wallCount) * i
			offsets[i] = Vector3.new(math.cos(angle) * radius, 0, math.sin(angle) * radius)
		end
	
		-- Delete all walls instantly
		for _, wall in ipairs(walls) do
			deleteRemote:FireServer(wall)
		end
	
		-- Build all walls instantly in circle
		for i, wall in ipairs(walls) do
			local wallPos = centerPos + offsets[i]
			local lookCFrame = CFrame.new(wallPos, centerPos)
			buildRemote:FireServer(wall.Name, lookCFrame)
		end
	end)
	
end
coroutine.wrap(UUMQM_fake_script)()
local function XIHZ_fake_script() -- Block.LocalScript 
	local script = Instance.new('LocalScript', Block)

	-- References
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = Players.LocalPlayer
	local targetInput = script.Parent.Parent
	local findButton = script.Parent
	
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	
	-- Find target by name or display name
	local function getTarget(name)
		for _, p in pairs(Players:GetPlayers()) do
			if p.Name:lower():find(name:lower()) 
				or p.DisplayName:lower():find(name:lower())
				or p.DisplayName:sub(1, #name):lower() == name:lower() then
				return p.Character
			end
		end
		return nil
	end
	
	-- Button click
	findButton.MouseButton1Click:Connect(function()
		local targetName = targetInput.Text
		local targetChar = getTarget(targetName)
	
		if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
			local hrp = targetChar.HumanoidRootPart
	
			-- Get 4 walls
			local walls = {}
			for _, wall in pairs(workspace.builds:GetChildren()) do
				if wall.Name == "Wall" then
					table.insert(walls, wall)
					if #walls >= 4 then break end
				end
			end
	
			if #walls < 4 then
				warn("Not enough walls found!")
				return
			end
	
			-- Delete the 4 walls
			deleteRemote:FireServer(unpack(walls))
			wait(0.1)
	
			-- Box offsets relative to target
			local offsets = {
				Vector3.new(2, 0, 2),
				Vector3.new(-2, 0, 2),
				Vector3.new(2, 0, -2),
				Vector3.new(-2, 0, -2),
			}
	
			-- Rebuild walls in box around target
			for i = 1, 4 do
				local wall = walls[i]
				local pos = hrp.Position + offsets[i]
				local blockCFrame = CFrame.new(pos)
				buildRemote:FireServer(wall.Name, blockCFrame)
			end
		else
			warn("Target not found or missing HRP!")
		end
	end)
end
coroutine.wrap(XIHZ_fake_script)()
local function PPPISX_fake_script() -- Tower.LocalScript 
	local script = Instance.new('LocalScript', Tower)

	-- Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	-- UI
	local textBox = script.Parent.Parent
	local trollButton = script.Parent
	
	-- Find player by short name or display name
	local function findPlayer(input)
		input = input:lower()
		for _, player in pairs(Players:GetPlayers()) do
			if player.Name:lower():sub(1, #input) == input
				or player.DisplayName:lower():sub(1, #input) == input then
				return player
			end
		end
		return nil
	end
	
	-- Troll function: delete all walls, then build a tower at HRP
	local function trollPlayer(target)
		if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
			local hrp = target.Character.HumanoidRootPart
			local buildsFolder = workspace:FindFirstChild("builds")
	
			if buildsFolder then
				for _, wall in pairs(buildsFolder:GetChildren()) do
					if wall.Name == "Wall" then
						for i = 1, 10 do
							deleteRemote:FireServer(wall)
							local offset = Vector3.new(0, i * 5, 0)
							buildRemote:FireServer("Wall", hrp.CFrame + offset)
							task.wait()
						end
					end
				end
			end
		end
	end
	
	-- Button logic
	trollButton.MouseButton1Click:Connect(function()
		local input = textBox.Text
		local target = findPlayer(input)
	
		trollPlayer(target)
	end)
end
coroutine.wrap(PPPISX_fake_script)()
local function GLIKQ_fake_script() -- LookAt.LocalScript 
	local script = Instance.new('LocalScript', LookAt)

	-- Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	-- UI elements
	local toggleButton = script.Parent
	local targetBox = script.Parent.Parent
	
	-- Toggle variable
	local toggled = false
	
	-- Function to make walls look at the target
	local function lookAtTarget(target)
		if not target or not target.Character then return end
		local hrp = target.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return end
	
		local buildsFolder = workspace:FindFirstChild("builds")
		if not buildsFolder then return end
	
		for _, wall in pairs(buildsFolder:GetChildren()) do
			if wall.Name == "Wall" then
				-- Delete the old wall
				deleteRemote:FireServer(wall)
				task.wait() -- slight wait to avoid spamming
				-- Rebuild facing the target
				buildRemote:FireServer(wall.Name, CFrame.new(wall.Position, hrp.Position))
			end
		end
	end
	
	-- Button click toggles the effect
	toggleButton.MouseButton1Click:Connect(function()
		toggled = not toggled
		toggleButton.Text = toggled and "Troll ON" or "Troll OFF"
	
		if toggled then
			spawn(function()
				while toggled do
					local targetName = targetBox.Text
					local target = Players:FindFirstChild(targetName)
					lookAtTarget(target)
					task.wait(0.5) -- frequency of updates
				end
			end)
		end
	end)
	
end
coroutine.wrap(GLIKQ_fake_script)()
local function BXGSLE_fake_script() -- TextButton_38.LocalScript 
	local script = Instance.new('LocalScript', TextButton_38)

	local player = game.Players.LocalPlayer
	local toggleButton = script.Parent
	local distBox = script.Parent.Parent.maxdist
	
	local RepStorage = game:GetService("ReplicatedStorage")
	local buildRemote = RepStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = RepStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	local spinning = false
	
	local function getMaxWallDistance()
		local buildsFolder = workspace:WaitForChild("builds")
		local maxDist = 0
		for _, build in ipairs(buildsFolder:GetChildren()) do
			if build:IsA("BasePart") and build.Name:find("Wall") then
				local dist = build.Position.Magnitude -- distance from origin
				if dist > maxDist then
					maxDist = dist
				end
			end
		end
		return maxDist
	end
	
	local function spinWallRandom(wall)
		deleteRemote:FireServer(wall)
	
		local pivot = wall:GetPivot()
		local randomYaw = math.random() * math.pi * 2
		local newCFrame = pivot * CFrame.Angles(0, randomYaw, 0)
	
		buildRemote:FireServer(wall.Name, newCFrame)
	end
	
	-- Toggle loop
	toggleButton.MouseButton1Click:Connect(function()
		spinning = not spinning
		toggleButton.Text = spinning and "STOP ROTATE ALL" or "ROTATE ALL"
	
		if spinning then
			task.spawn(function()
				while spinning do
					local buildsFolder = workspace:WaitForChild("builds")
	
					-- Distance from textbox
					local dist = tonumber(distBox.Text) or 0
	
					-- Max distance among all Walls
					local maxWallDist = getMaxWallDistance()
	
					-- Only rotate if dist <= maxWallDist
					if dist <= maxWallDist then
						for _, wall in ipairs(buildsFolder:GetChildren()) do
							if wall.Name:find("Wall") then
								spinWallRandom(wall)
							end
						end
					end
	
					task.wait(0.2)
				end
			end)
		end
	end)
end
coroutine.wrap(BXGSLE_fake_script)()
local function NQUVUR_fake_script() -- TextButton_39.LocalScript 
	local script = Instance.new('LocalScript', TextButton_39)

	--// GUI Setup
	local player = game.Players.LocalPlayer
	local toggleButton = script.Parent
	
	-- Remotes
	local RepStorage = game:GetService("ReplicatedStorage")
	local buildRemote = RepStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = RepStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	-- Toggle state
	local moving = false
	
	-- Helper function to move wall randomly
	local function moveWallRandom(wall)
		-- Only proceed if the name contains "Wall"
		if wall.Name == "Wall" then
	
		-- Delete existing wall
		deleteRemote:FireServer(wall)
		task.wait(0.1)
	
		-- Get wall center using GetPivot (works for models)
		local wallPivot = wall:GetPivot()
	
		-- Random offset
		local offset = Vector3.new(
			math.random(-5,5),  -- X offset
			0,                  -- Y stays same
			math.random(-5,5)   -- Z offset
		)
	
		-- Build at new CFrame
		local newCFrame = CFrame.new(wallPivot + offset)
			buildRemote:FireServer(wall.Name, newCFrame)
		end
	end
	
	-- Loop toggle logic
	toggleButton.MouseButton1Click:Connect(function()
		moving = not moving
		toggleButton.Text = moving and "STOP MOVE ALL RANDOM" or "MOVE ALL RANDOM"
	
		if moving then
			spawn(function()
				while moving do
					local buildsFolder = workspace:WaitForChild("builds")
					for _, wall in pairs(buildsFolder:GetChildren()) do
						moveWallRandom(wall)
						task.wait(0.1)
					end
					task.wait(0.2) -- wait before next random move
				end
			end)
		end
	end)
end
coroutine.wrap(NQUVUR_fake_script)()
local function SOOCRYR_fake_script() -- TextButton_40.LocalScript 
	local script = Instance.new('LocalScript', TextButton_40)

	--// GUI Setup
	local player = game.Players.LocalPlayer
	local frame = script.Parent
	local moveButton = script.Parent
	local distBox = script.Parent.Parent.maxdist
	
	-- Remotes
	local RepStorage = game:GetService("ReplicatedStorage")
	local buildRemote = RepStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = RepStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	-- Button click logic
	moveButton.MouseButton1Click:Connect(function()
		local buildsFolder = workspace:WaitForChild("builds")
	
		-- Get distance from TextBox
		local maxDist = tonumber(distBox.Text) or 0
	
		for _, wall in pairs(buildsFolder:GetChildren()) do
			if wall.Name == "Wall" then
				-- Only move if wall distance <= TextBox value
				local wallDist = wall.Position.Magnitude -- distance from origin
				if wallDist <= maxDist then
					-- Delete old wall
					deleteRemote:FireServer(wall)
					task.wait(0.05)
	
					-- Get pivot CFrame (keeps rotation)
					local wallCFrame = wall:GetPivot()
	
					-- Move Y +5
					local newCFrame = wallCFrame + Vector3.new(0, 5, 0)
	
					-- Fire build remote
					buildRemote:FireServer(wall.Name, newCFrame)
				end
			end
		end
	end)
end

		D = { "0111111000","0100000100","0100000010","0100000010","0100000010","0100000010","0100000010","0100000100","0111111000","0000000000" },
		E = { "0111111110","0100000000","0100000000","0100000000","0111111100","0100000000","0100000000","0100000000","0111111110","0000000000" },
		F = { "0111111110","0100000000","0100000000","0100000000","0111111100","0100000000","0100000000","0100000000","0100000000","0000000000" },
		G = { "0001111100","0010000010","0100000000","0100000000","0100111110","0100000010","0100000010","0010000010","0001111100","0000000000" },
		H = { "0100000010","0100000010","0100000010","0100000010","0111111110","0100000010","0100000010","0100000010","0100000010","0000000000" },
		I = { "0011111000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0011111000","0000000000" },
		J = { "0001111100","0000010000","0000010000","0000010000","0000010000","0000010000","0100010000","0100010000","0011100000","0000000000" },
		K = { "0100000100","0100001000","0100010000","0100100000","0111000000","0100100000","0100010000","0100001000","0100000100","0000000000" },
		L = { "0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0111111110","0000000000" },
		M = { "0100000010","0110000110","0101001010","0100100010","0100000010","0100000010","0100000010","0100000010","0100000010","0000000000" },
		N = { "0100000010","0110000010","0101000010","0100100010","0100010010","0100001010","0100000110","0100000010","0100000010","0000000000" },
		O = { "0001111000","0010000100","0100000010","0100000010","0100000010","0100000010","0100000010","0010000100","0001111000","0000000000" },
		P = { "0111111000","0100000100","0100000010","0100000010","0111111000","0100000000","0100000000","0100000000","0100000000","0000000000" },
		Q = { "0001111000","0010000100","0100000010","0100000010","0100000010","0100100010","0100010010","0010000100","0001111010","0000000000" },
		R = { "0111111000","0100000100","0100000010","0100000010","0111111000","0100100000","0100010000","0100001000","0100000100","0000000000" },
		S = { "0011111100","0100000010","0100000000","0011111000","0000000100","0000000010","0100000010","0100000100","0011111000","0000000000" },
		T = { "0111111110","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0000000000" },
		U = { "0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0011111100","0000000000" },
		V = { "0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0010000100","0001010000","0000100000","0000000000" },
		W = { "0100000010","0100000010","0100000010","0100000010","0100100010","0100100010","0101010100","0010001000","0010001000","0000000000" },
		X = { "0100000010","0010000100","0001010000","0000100000","0000100000","0001010000","0010000100","0100000010","0100000010","0000000000" },
		Y = { "0100000010","0010000100","0001010000","0000100000","0000100000","0000100000","0000100000","0000100000","0000100000","0000000000" },
		Z = { "0111111110","0000000100","0000001000","0000010000","0000100000","0001000000","0010000000","0100000000","0111111110","0000000000" }
	}

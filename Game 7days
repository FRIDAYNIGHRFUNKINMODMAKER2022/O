								task.wait(.2)
	
								-- Fire prompt
								if running then
									fireproximityprompt(prompt)
								else
									break
								end
							end
						end
					end
				end
			end
			task.wait(LOOP_DELAY)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		running = not running
		if running then
			button.Text = "STOP AUTO PEBBLES"
			-- Save old position
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				oldCFrame = player.Character.HumanoidRootPart.CFrame
			end
			task.spawn(doHarvestLoop)
		else
			button.Text = "AUTO PEBBLES"
			-- Teleport back instantly
			if oldCFrame and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				player.Character.HumanoidRootPart.CFrame = oldCFrame
			end
		end
	end)
	
end
coroutine.wrap(FXMT_fake_script)()
local function QUGYTK_fake_script() -- TextButton_29.LocalScript 
	local script = Instance.new('LocalScript', TextButton_29)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	local camera = workspace.CurrentCamera
	local humanoid = character:WaitForChild("Humanoid")
	
	local toggle = false
	local targetTree = nil
	local oldPos = nil
	local cameraY = nil
	local yOffset = 10 -- teleport 10 studs above trunk
	
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		toggle = not toggle
		if toggle then
			oldPos = hrp.Position -- store original player position
			cameraY = camera.CFrame.Position.Y -- store camera Y
			targetTree = nil -- reset target
			button.Text = "STOP AUTO TREE"
		else
			if oldPos then
				hrp.CFrame = CFrame.new(oldPos)
			end
			targetTree = nil
			button.Text = "AUTO TREE"
		end
	end)
	
	-- Function to find nearest tree with hp > 0
	local function getNearestTree()
		local nearest = nil
		local minDist = math.huge
		for _, tree in pairs(workspace:FindFirstChild("trees") and workspace.trees:GetChildren() or {}) do
			local trunk = tree:FindFirstChild("Trunk")
			local hp = tree:FindFirstChild("hp")
			if trunk and hp and hp.Value > 0 then
				local dist = (trunk.Position - hrp.Position).Magnitude
				if dist < minDist then
					minDist = dist
					nearest = tree
				end
			end
		end
		return nearest
	end
	
	-- Main loop
	RunService.RenderStepped:Connect(function()
		if toggle and oldPos then
			-- Pick a new tree if none or current is dead
			if not targetTree or not targetTree:FindFirstChild("hp") or targetTree.hp.Value <= 0 then
				targetTree = getNearestTree()
				if targetTree then
					local trunk = targetTree:FindFirstChild("Trunk")
					if trunk then
						-- Teleport X/Z same, Y = trunk.Y + yOffset
						local pos = trunk.Position
						hrp.CFrame = CFrame.new(pos.X, pos.Y + yOffset, pos.Z)
					end
				end
			end
	
			-- Camera look at trunk, keeping original Y
			if targetTree and targetTree:FindFirstChild("Trunk") then
				local trunk = targetTree.Trunk
				local camPos = camera.CFrame.Position
				local lookAt = Vector3.new(trunk.Position.X, camPos.Y, trunk.Position.Z)
				camera.CFrame = CFrame.new(camPos, lookAt)
			end
		end
	end)
end
coroutine.wrap(QUGYTK_fake_script)()
local function PEXR_fake_script() -- TextButton_30.LocalScript 
	local script = Instance.new('LocalScript', TextButton_30)

	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = Players.LocalPlayer
	local PlayerGui = player:WaitForChild("PlayerGui")
	local button = script.Parent
	local textBox = script.Parent.TextBox
	
	local autoFuel = false
	local remote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("add_fuel")
	
	local function ensureWoodEquipped()
		local wood = player.Backpack:FindFirstChild("Wood")
		if wood then
			wood.Parent = player.Character
			return true
		end
		wood = player.Character and player.Character:FindFirstChild("Wood")
		if wood then
			return true
		end
		return false
	end
	
	-- Toggle loop
	button.MouseButton1Click:Connect(function()
		autoFuel = not autoFuel
		button.Text = autoFuel and "STOP AUTO WOOD" or "AUTO WOOD"
	
		if autoFuel then
			task.spawn(function()
				while autoFuel do
					local builds = workspace:FindFirstChild("builds")
					if builds then
						local campfire = builds:FindFirstChild("Campfire")
						if campfire and campfire:FindFirstChild("fuel") then
							local fuel = campfire.fuel
	
							-- get threshold from textbox
							local threshold = tonumber(textBox.Text) or 0
	
							if fuel.Value <= threshold then
								if ensureWoodEquipped() then
									while autoFuel and fuel.Value <= threshold do
										remote:FireServer(campfire)
										task.wait(0.2)
									end
								else
									warn(" No Wood found in Backpack or Character!")
								end
							end
						end
					end
					task.wait(0.3)
				end
			end)
		end
	end)
end
coroutine.wrap(PEXR_fake_script)()
local function WGSCS_fake_script() -- TextButton_31.LocalScript 
	local script = Instance.new('LocalScript', TextButton_31)

	--// Services
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	
	--// Variables
	local toggled = false
	local oldPos = nil
	local button = script.Parent
	
	--// Loop function
	local function loopAction()
		oldPos = player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character.HumanoidRootPart.CFrame
	
		while toggled do
			if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then break end
	
			local droppedFolder = workspace:WaitForChild("dropped")
			local items = droppedFolder:GetChildren()
	
			for _, item in pairs(items) do
				local target = item:FindFirstChild("Handle") or item:FindFirstChild("main")
				if target then
					-- Move player to target
					player.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0, 2.3, 0)
	
					-- Make camera look at the target instantly
					camera.CFrame = CFrame.lookAt(camera.CFrame.Position, target.Position)
	
					-- Trigger all ProximityPrompts manually
					for _, prox in pairs(target:GetChildren()) do
						if prox:IsA("ProximityPrompt") and prox.Enabled then
							prox.HoldDuration = 0
							prox:InputHoldBegin()
							prox:InputHoldEnd()
						end
					end
	
					-- Wait until the target is removed (picked up) before moving on
					repeat
						RunService.Heartbeat:Wait()
					until not target or not target.Parent
	
					-- Small delay before going to next item
					wait(0.1)
				end
			end
	
			RunService.Heartbeat:Wait()
		end
	
		-- Return to old position when toggled off
		if oldPos and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			player.Character.HumanoidRootPart.CFrame = oldPos
		end
	end
	
	--// Button click
	button.MouseButton1Click:Connect(function()
		toggled = not toggled
		if toggled then
			task.spawn(loopAction) -- run in a separate thread
		end
	end)
end
coroutine.wrap(WGSCS_fake_script)()
local function TRYRDSO_fake_script() -- TextButton_32.LocalScript 
	local script = Instance.new('LocalScript', TextButton_32)

	local player = game.Players.LocalPlayer
	local workspace = game:GetService("Workspace")
	local toggle = false -- loop toggle
	local button = script.Parent
	
	-- Button click logic
	button.MouseButton1Click:Connect(function()
		toggle = not toggle
		button.Text = toggle and "STOP AUTO HELI-CRASH" or "AUTO HELI-CRASH"
	
		if toggle then
			spawn(function()
				while toggle do
					if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
						task.wait(0.5)
						continue
					end
	
					local oldPos = player.Character.HumanoidRootPart.CFrame
	
					if workspace:FindFirstChild("interact") then
						for _, obj in pairs(workspace.interact:GetChildren()) do
							if obj.Name == "heli_crash" then
								local body = obj:FindFirstChild("Body")
								if body then
									-- Teleport to body
									player.Character.HumanoidRootPart.CFrame = body.CFrame + Vector3.new(0,3,0)
	
									-- Fire ProximityPrompt directly
									for _, prompt in pairs(body:GetDescendants()) do
										if prompt:IsA("ProximityPrompt") then
											prompt:FireProximityPrompt(player)
										end
									end
	
									task.wait(0.2)
								end
							end
						end
					end
	
					-- Return to old position
					player.Character.HumanoidRootPart.CFrame = oldPos
					task.wait(1) -- small delay before next loop
				end
			end)
		end
	end)
end
coroutine.wrap(TRYRDSO_fake_script)()
local function WOQMRF_fake_script() -- TextButton_33.LocalScript 
	local script = Instance.new('LocalScript', TextButton_33)

	local button = script.Parent
	local toggleLoop = false
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	
	-- Helper function to teleport and fire ProximityPrompt
	local function handleStructure(structure, oldPos)
		if not structure or not structure.Parent then return false end -- skip if removed
	
		local log = structure:FindFirstChild("log")
		if log then
			-- Teleport player to log
			if hrp then
				hrp.CFrame = log.CFrame + Vector3.new(0, 1.3, 0)
			end
	
			-- Find "main" and fire ProximityPrompt inside
			local main = log:FindFirstChild("main")
			if main then
				for _, prox in pairs(main:GetDescendants()) do
					if prox:IsA("ProximityPrompt") then
						prox:FireProximityPrompt(player)
					end
				end
			end
	
			wait(0.5) -- wait before next teleport
			return true
		else
			return false -- log not found
		end
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggleLoop = not toggleLoop
		button.Text = toggleLoop and "STOP AUTO LOG" or "AUTO LOG"
	
		if toggleLoop then
			spawn(function()
				local oldPos = hrp.CFrame
				while toggleLoop do
					local logs = {}
	
					-- Collect all valid logs
					for _, obj in pairs(workspace:GetDescendants()) do
						if obj.Name == "cabin" or obj.Name == "abandoned_shack" then
							if obj:FindFirstChild("log") then
								table.insert(logs, obj)
							end
						end
					end
	
					-- Teleport 1 by 1, remove invalid logs
					local i = 1
					while i <= #logs do
						local structure = logs[i]
						local valid = handleStructure(structure, oldPos)
						if not valid then
							table.remove(logs, i) -- remove invalid log
						else
							i = i + 1
						end
					end
	
					wait(1)
				end
			end)
		end
	end)
end
coroutine.wrap(WOQMRF_fake_script)()
local function PLRO_fake_script() -- TextButton_34.LocalScript 
	local script = Instance.new('LocalScript', TextButton_34)

	--// Services
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	
	--// Toggle variable
	local looping = false
	local oldPos = nil
	
	local Button = script.Parent
	
	--// Button toggle
	Button.MouseButton1Click:Connect(function()
		looping = not looping
		
		if looping == false then
			script.Parent.Text = "STOP AUTO GLASS"
		else
			script.Parent.Text = "AUTO GLASS"
		end
		
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		local head = player.Character and player.Character:FindFirstChild("Head")
	
		if looping then
			-- Store original position only
			if hrp then
				oldPos = hrp.CFrame
			end
		else
			-- First teleport to old position
			if hrp and oldPos then
				hrp.CFrame = oldPos
				oldPos = nil
			end
	
			-- Anchor both
			if hrp then hrp.Anchored = true end
			if head then head.Anchored = true end
	
			-- Wait 0.1 seconds
			wait(0.1)
	
			-- Unanchor both
			if hrp then hrp.Anchored = false end
			if head then head.Anchored = false end
		end
	
		Button.Text = looping and "Stop Glass Loop" or "Start Glass Loop"
	end)
	
	--// Function to find nearest MeshPart
	local function getNearestGlassMesh()
		local nearest = nil
		local shortestDistance = math.huge
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return nil end
	
		for _, obj in pairs(workspace.interact:GetDescendants()) do
			if obj:IsA("Model") and obj.Name:lower() == "glass" then
				for _, part in pairs(obj:GetDescendants()) do
					if part:IsA("MeshPart") then
						local distance = (hrp.Position - part.Position).Magnitude
						if distance < shortestDistance then
							shortestDistance = distance
							nearest = part
						end
					end
				end
			end
		end
	
		return nearest
	end
	
	--// Main function
	local function interactWithNearestGlass()
		local part = getNearestGlassMesh()
		if part and player.Character then
			local hrp = player.Character:FindFirstChild("HumanoidRootPart")
			if not hrp then return end
	
			-- Check if part.Position is valid
			local pos = part.Position
			if not pos or pos.X ~= pos.X or pos.Y ~= pos.Y or pos.Z ~= pos.Z then
				return -- skip invalid position
			end
	
			-- Teleport HumanoidRootPart directly to the part
			hrp.CFrame = CFrame.new(pos)
	
			-- Make the camera look at the MeshPart without moving its position
			local cam = workspace.CurrentCamera
			cam.CFrame = CFrame.new(cam.CFrame.Position, pos)
	
			-- Fire any ProximityPrompt inside the part
			for _, prompt in pairs(part:GetDescendants()) do
				if prompt:IsA("ProximityPrompt") then
					prompt.HoldDuration = 0
					prompt:InputHoldBegin()
					prompt:InputHoldEnd()
				end
			end
		end
	end
	
	--// Loop runner using Heartbeat
	RunService.Heartbeat:Connect(function()
		if looping then
			interactWithNearestGlass()
			wait(0.1) -- small delay between interactions
		end
	end)
end
coroutine.wrap(PLRO_fake_script)()
local function PSAFNS_fake_script() -- TextButton_35.LocalScript 
	local script = Instance.new('LocalScript', TextButton_35)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local button = script.Parent
	
	-- Send notification
	local function notify(title, text)
		game.StarterGui:SetCore("SendNotification", {Title = title, Text = text, Duration = 3})
	end
	
	-- Find nearest well
	local function getNearestWell()
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return nil end
	
		local nearestWell = nil
		local shortestDistance = math.huge
	
		for _, well in pairs(workspace:GetDescendants()) do
			if well.Name == "well" then
				local main = well:FindFirstChild("main")
				local prox = main and main:FindFirstChildWhichIsA("ProximityPrompt")
				if main and prox then
					local dist = (hrp.Position - main.Position).Magnitude
					if dist < shortestDistance then
						shortestDistance = dist
						nearestWell = {main = main, prox = prox}
					end
				end
			end
		end
	
		return nearestWell
	end
	
	-- Get Glass from Character or Backpack
	local function getGlass()
		local glass = player.Character and player.Character:FindFirstChild("Glass")
		if glass then return glass end
	
		glass = player.Backpack:FindFirstChild("Glass")
		if glass and player.Character then
			player.Character.Humanoid:EquipTool(glass)
			return glass
		end
	
		return nil
	end
	
	-- Fire ProximityPrompt until it registers or Glass disappears
	local function fireProx(target)
		while target.prox and target.prox.Enabled do
			-- Check if Glass still exists in Character
			if not player.Character:FindFirstChild("Glass") then
				notify("Glass Missing", "Glass disappeared from your Character. Action stopped.")
				return false
			end
	
			local hrp = player.Character:FindFirstChild("HumanoidRootPart")
			if not hrp then break end
	
			-- Teleport to main and look at it
			hrp.CFrame = CFrame.new(target.main.Position + Vector3.new(0, 3, 0))
			workspace.CurrentCamera.CFrame = CFrame.new(hrp.Position, target.main.Position)
	
			-- Fire ProximityPrompt
			target.prox:InputHoldBegin()
			wait(0.1)
			target.prox:InputHoldEnd()
	
			-- Check if still enabled (means it didn't register)
			if not target.prox.Enabled then
				break
			end
	
			wait(0.2) -- retry if not fired
		end
		return true
	end
	
	-- Main action
	local function runOnceIfGlass()
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then
			notify("Error", "HumanoidRootPart not found!")
			return
		end
	
		local oldPos = hrp.CFrame
	
		-- Check for Glass
		local glass = getGlass()
		if not glass then
			notify("No Glass", "You don't have 'Glass' in Character or Backpack.")
			return
		end
	
		local target = getNearestWell()
		if target then
			local success = fireProx(target)
			-- Only teleport back if ProximityPrompt fired successfully
			if success then
				hrp.CFrame = oldPos
				notify("Action Complete", "Glass successfully used on nearest well.")
			else
				-- Glass disappeared before firing
				hrp.CFrame = oldPos
			end
		else
			notify("No Well Found", "No well with 'main' part found in Workspace.")
		end
	end
	
	-- Button click
	button.MouseButton1Click:Connect(function()
		runOnceIfGlass()
	end)
end
coroutine.wrap(PSAFNS_fake_script)()
local function HTCQVWY_fake_script() -- TextButton_36.LocalScript 
	local script = Instance.new('LocalScript', TextButton_36)

	-- LocalScript inside GUI button
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local toggled = false
	local loop
	
	-- Food names we care about
	local foodList = {
		"Berry",
		"Carrot",
		"Cooked Deer Meat",
		"Beans"
	}
	
	-- Function to eat food if hunger <= 10
	local function eatFood()
		local char = player.Character or player.CharacterAdded:Wait()
		local humanoid = char:FindFirstChildOfClass("Humanoid")
		local hunger = char:FindFirstChild("hunger")
	
		if not humanoid or not hunger then return end
	
		-- Only eat if hunger <= 10
		if hunger.Value > 10 then return end
	
		local ate = false
	
		-- 1 Check equipped tools
		for _, tool in ipairs(char:GetChildren()) do
			if tool:IsA("Tool") and table.find(foodList, tool.Name) then
				local eatRemote = tool:FindFirstChild("eat")
				if eatRemote and eatRemote:IsA("RemoteEvent") then
					eatRemote:FireServer()
					ate = true
					task.wait(0.3)
					break
				end
			end
		end
	
		-- 2 If no food equipped, check backpack
		if not ate then
			local backpack = player:WaitForChild("Backpack")
			for _, name in ipairs(foodList) do
				local food = backpack:FindFirstChild(name)
				if food and food:IsA("Tool") then
					humanoid:EquipTool(food)
					local eatRemote = food:FindFirstChild("eat")
					if eatRemote and eatRemote:IsA("RemoteEvent") then
						eatRemote:FireServer()
						task.wait(0.3)
						break
					end
				end
			end
		end
	end
	
	-- Loop function
	local function startLoop()
		loop = task.spawn(function()
			while toggled do
				eatFood()
				task.wait(1) -- check every second
			end
		end)
	end
	
	-- Button toggle
	button.MouseButton1Click:Connect(function()
		toggled = not toggled
		if toggled then
			button.Text = "STOP AUTO EAT"
			startLoop()
		else
			button.Text = "AUTO EAT"
		end
	end)
end
coroutine.wrap(HTCQVWY_fake_script)()

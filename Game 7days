coroutine.wrap(DMYCETM_fake_script)()
local function SNOOGFR_fake_script() -- TextButton_42.LocalScript 
	local script = Instance.new('LocalScript', TextButton_42)

	-- Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local workspaceBuilds = workspace:WaitForChild("builds")
	
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	
	-- GUI
	local textBox = script.Parent.TextBox
	local buildButton = script.Parent
	
	-- Remotes
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	
	-- Letter patterns (10x10)
	local letters = {
		A = { "0001111000","0010000100","0100000010","0100000010","0111111110","0100000010","0100000010","0100000010","0100000010","0000000000" },
		B = { "0111111100","0100000010","0100000010","0100000010","0111111100","0100000010","0100000010","0100000010","0111111100","0000000000" },
		C = { "0001111100","0010000010","0100000000","0100000000","0100000000","0100000000","0100000000","0010000010","0001111100","0000000000" },
		D = { "0111111000","0100000100","0100000010","0100000010","0100000010","0100000010","0100000010","0100000100","0111111000","0000000000" },
		E = { "0111111110","0100000000","0100000000","0100000000","0111111100","0100000000","0100000000","0100000000","0111111110","0000000000" },
		F = { "0111111110","0100000000","0100000000","0100000000","0111111100","0100000000","0100000000","0100000000","0100000000","0000000000" },
		G = { "0001111100","0010000010","0100000000","0100000000","0100111110","0100000010","0100000010","0010000010","0001111100","0000000000" },
		H = { "0100000010","0100000010","0100000010","0100000010","0111111110","0100000010","0100000010","0100000010","0100000010","0000000000" },
		I = { "0011111000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0011111000","0000000000" },
		J = { "0001111100","0000010000","0000010000","0000010000","0000010000","0000010000","0100010000","0100010000","0011100000","0000000000" },
		K = { "0100000100","0100001000","0100010000","0100100000","0111000000","0100100000","0100010000","0100001000","0100000100","0000000000" },
		L = { "0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0100000000","0111111110","0000000000" },
		M = { "0100000010","0110000110","0101001010","0100100010","0100000010","0100000010","0100000010","0100000010","0100000010","0000000000" },
		N = { "0100000010","0110000010","0101000010","0100100010","0100010010","0100001010","0100000110","0100000010","0100000010","0000000000" },
		O = { "0001111000","0010000100","0100000010","0100000010","0100000010","0100000010","0100000010","0010000100","0001111000","0000000000" },
		P = { "0111111000","0100000100","0100000010","0100000010","0111111000","0100000000","0100000000","0100000000","0100000000","0000000000" },
		Q = { "0001111000","0010000100","0100000010","0100000010","0100000010","0100100010","0100010010","0010000100","0001111010","0000000000" },
		R = { "0111111000","0100000100","0100000010","0100000010","0111111000","0100100000","0100010000","0100001000","0100000100","0000000000" },
		S = { "0011111100","0100000010","0100000000","0011111000","0000000100","0000000010","0100000010","0100000100","0011111000","0000000000" },
		T = { "0111111110","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0001000000","0000000000" },
		U = { "0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0011111100","0000000000" },
		V = { "0100000010","0100000010","0100000010","0100000010","0100000010","0100000010","0010000100","0001010000","0000100000","0000000000" },
		W = { "0100000010","0100000010","0100000010","0100000010","0100100010","0100100010","0101010100","0010001000","0010001000","0000000000" },
		X = { "0100000010","0010000100","0001010000","0000100000","0000100000","0001010000","0010000100","0100000010","0100000010","0000000000" },
		Y = { "0100000010","0010000100","0001010000","0000100000","0000100000","0000100000","0000100000","0000100000","0000100000","0000000000" },
		Z = { "0111111110","0000000100","0000001000","0000010000","0000100000","0001000000","0010000000","0100000000","0111111110","0000000000" }
	}
	
	local blockSize = 6
	local letterSize = 10
	
	-- Get all walls
	local function getWalls()
		local walls = {}
		for _, wall in pairs(workspaceBuilds:GetChildren()) do
			if wall.Name == "Wall" then
				table.insert(walls, wall)
			end
		end
		return walls
	end
	
	-- Count how many blocks are needed for a letter
	local function countBlocks(letterChar)
		local pattern = letters[letterChar:upper()]
		if not pattern then return 0 end
		local count = 0
		for _, line in ipairs(pattern) do
			for i = 1, #line do
				if line:sub(i,i) == "1" then
					count = count + 1
				end
			end
		end
		return count
	end
	
	-- Build a single letter
	local function buildLetter(letterChar, startPos, walls)
		local pattern = letters[letterChar:upper()]
		if not pattern then return 0 end
		local wallIndex = 1
		for row = 1, #pattern do
			local line = pattern[row]
			for col = 1, #line do
				if line:sub(col,col) == "1" then
					if wallIndex > #walls then return wallIndex-1 end
					local pos = startPos + Vector3.new((col-1)*blockSize,0,(row-1)*blockSize)
					buildRemote:FireServer(walls[wallIndex].Name,pos)
					wallIndex = wallIndex + 1
				end
			end
		end
		return wallIndex-1
	end
	
	-- Build button click
	buildButton.MouseButton1Click:Connect(function()
		local inputText = textBox.Text
		if inputText == "" then return end
		local walls = getWalls()
		if #walls == 0 then return end
	
		local startPos = hrp.Position + Vector3.new(0,5,0)
		local offsetX = 0
	
		for i = 1, #inputText do
			local letter = inputText:sub(i,i)
			local needed = countBlocks(letter)
			local wallsToUse = {}
			for j = 1, math.min(needed, #walls) do
				table.insert(wallsToUse, walls[j])
			end
			if #wallsToUse == 0 then break end
	
			-- Delete walls one by one
			for _, wall in ipairs(wallsToUse) do
				deleteRemote:FireServer(wall)
				wait(0.05) -- small delay to ensure remote fires properly
			end
	
			-- Build the letter
			buildLetter(letter, startPos + Vector3.new(offsetX,0,0), wallsToUse)
			offsetX = offsetX + letterSize * blockSize + 2
	
			-- Remove used walls from main list
			for j = 1, #wallsToUse do table.remove(walls, 1) end
		end
	end)
end
coroutine.wrap(SNOOGFR_fake_script)()
local function BWURE_fake_script() -- TextBox_6.Script 
	local script = Instance.new('Script', TextBox_6)

	local letters = {
		A = { "0001111000","0010000100","0100000010","0100000010","0111111110","0100000010","0100000010","0100000010","0100000010","0000000000" },
		B = { "0111111100","0100000010","0100000010","0100000010","0111111100","0100000010","0100000010","0100000010","0111111100","0000000000" },

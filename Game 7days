coroutine.wrap(BWURE_fake_script)()
local function LKMTQK_fake_script() -- TextButton_43.LocalScript 
	local script = Instance.new('LocalScript', TextButton_43)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	local player = Players.LocalPlayer
	local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
	local buildsFolder = workspace:FindFirstChild("builds")
	
	script.Parent.MouseButton1Click:Connect(function()
		if hrp and buildsFolder then
			for _, wall in pairs(buildsFolder:GetChildren()) do
				if wall.Name == "Wall" then
					for i = 1, 10 do
						deleteRemote:FireServer(wall)
						task.wait()
						buildRemote:FireServer(wall.Name, CFrame.new(wall.Position + Vector3.new(0, i*5, 0)))
					end
				end
			end
		end
	end)
end
coroutine.wrap(LKMTQK_fake_script)()
local function XTPHXKD_fake_script() -- LookAt_2.LocalScript 
	local script = Instance.new('LocalScript', LookAt_2)

	-- Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local buildRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("build")
	local deleteRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("delete")
	
	-- UI
	local trollButton = script.Parent
	
	-- Toggle variable
	local toggled = false
	
	-- Function to make all walls look at LocalPlayer
	local function lookAtLocalPlayerHRP()
		local localPlayer = Players.LocalPlayer
		if not localPlayer or not localPlayer.Character then return end
		local hrp = localPlayer.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return end
	
		local buildsFolder = workspace:FindFirstChild("builds")
		if not buildsFolder then return end
	
		for _, wall in pairs(buildsFolder:GetChildren()) do
			if wall.Name == "Wall" then
				-- Delete wall
				deleteRemote:FireServer(wall)
				-- Rebuild facing LocalPlayer
				buildRemote:FireServer(wall.Name, CFrame.new(wall.Position, hrp.Position))
			end
		end
	end
	
	-- Button click toggles the feature
	trollButton.MouseButton1Click:Connect(function()
		toggled = not toggled
		trollButton.Text = toggled and "STOP LOOK AT" or "LOOK AT"
	
		if toggled then
			-- Run in a loop while toggled is true
			spawn(function()
				while toggled do
					lookAtLocalPlayerHRP()
					wait(0.5) -- adjust frequency as needed
				end
			end)
		end
	end)
end
coroutine.wrap(XTPHXKD_fake_script)()
local function XYAN_fake_script() -- TextButton_44.LocalScript 
	local script = Instance.new('LocalScript', TextButton_44)

	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	local cam = workspace.CurrentCamera
	local minPos = Vector3.new(418, 27, 2102)
	local maxPos = Vector3.new(2155, 40, 542)
	local yOffset = 12
	local runToggle = false
	local oldPos = nil
	local visitedRandomPositions = {}
	local button = script.Parent
	
	-- Generate unique random position
	local function getUniqueRandomPos()
		local pos
		local tries = 0
		repeat
			pos = Vector3.new(
				minPos.X + math.random() * (maxPos.X - minPos.X),
				minPos.Y + math.random() * (maxPos.Y - minPos.Y) + yOffset,
				minPos.Z + math.random() * (maxPos.Z - minPos.Z)
			)
	
			local unique = true
			for _, v in pairs(visitedRandomPositions) do
				if (pos - v).Magnitude < 0.1 then
					unique = false
					break
				end
			end
			tries = tries + 1
		until unique or tries > 50
	
		table.insert(visitedRandomPositions, pos)
	
		-- If all positions visited, delete all
		if #visitedRandomPositions >= 50 then
			visitedRandomPositions = {}
		end
	
		return pos
	end
	
	-- Find all heli_crash
	local function findHelis()
		local list = {}
		for _, obj in ipairs(workspace.interact:GetChildren()) do
			if obj.Name == "heli_crash" then
				local body = obj:FindFirstChild("Body")
				local prox = body and body:FindFirstChildWhichIsA("ProximityPrompt")
				if body then
					table.insert(list, {obj = obj, body = body, prox = prox})
				end
			end
		end
		return list
	end
	
	-- Find closest heli
	local function getClosestHeli(heliList)
		local closest = nil
		local shortest = math.huge
		for _, heli in ipairs(heliList) do
			local dist = (hrp.Position - heli.body.Position).Magnitude
			if dist < shortest then
				shortest = dist
				closest = heli
			end
		end
		return closest
	end
	
	-- Main TP loop
	local function tpLoop()
		while runToggle do
			local heliList = findHelis()
	
			if #heliList > 0 then
				local heli = getClosestHeli(heliList)
	
				-- Notification when a heli_crash is found
				game.StarterGui:SetCore("SendNotification", {
					Title = "Heli Found",
					Text = "Found a HeliCrash!",
					Duration = 3
				})
	
				-- Loop teleport near closest heli
				while runToggle and heli.obj.Parent and workspace.interact:FindFirstChild(heli.obj.Name) do
					local targetPos = heli.body.Position + (heli.body.CFrame.LookVector * -2)
					hrp.CFrame = CFrame.new(targetPos, heli.body.Position)
	
					-- Camera looks at heli with short wait
					cam.CFrame = CFrame.new(cam.CFrame.Position, heli.body.Position)
					task.wait(0.1)
	
					if heli.prox then
						heli.prox.HoldDuration = 0
						heli.prox:InputHoldBegin()
						heli.prox:InputHoldEnd()
					end
	
					task.wait(0.05)
				end
			else
				-- Random TP
				hrp.CFrame = CFrame.new(getUniqueRandomPos())
				task.wait(0.05)
			end
		end
	end
	
	-- Button toggle
	button.MouseButton1Click:Connect(function()
		runToggle = not runToggle
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
	
		if runToggle then
			button.Text = "STOP"
			if not oldPos then
				oldPos = hrp.CFrame
			end
			task.spawn(tpLoop)
		else
			button.Text = "FARM HELI-CRASH (RANDOM TP)"
			if oldPos then
				hrp.CFrame = oldPos
				oldPos = nil
			end
		end
	end)
end
coroutine.wrap(XYAN_fake_script)()
local function USYUWD_fake_script() -- TextButton_45.LocalScript 
	local script = Instance.new('LocalScript', TextButton_45)

	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	local cam = workspace.CurrentCamera
	
	-- TP area
	local minPos = Vector3.new(418, 27, 2102)
	local maxPos = Vector3.new(2155, 40, 542)
	local yOffset = 12
	local runToggle = false
	local oldPos = nil
	local gridStep = 20
	local currentX = minPos.X
	local currentZ = minPos.Z
	local button = script.Parent
	
	-- Generate next line/grid TP position
	local function getNextGridPos()
		local pos = Vector3.new(currentX, minPos.Y + yOffset, currentZ)
		currentX += gridStep
		if currentX > maxPos.X then
			currentX = minPos.X
			currentZ += gridStep
			if currentZ > maxPos.Z then
				-- Reset after completing the grid
				currentZ = minPos.Z
			end
		end
		return pos
	end
	
	-- Find all heli_crash
	local function findHelis()
		local list = {}
		for _, obj in ipairs(workspace.interact:GetChildren()) do
			if obj.Name == "heli_crash" then
				local body = obj:FindFirstChild("Body")
				local prox = body and body:FindFirstChildWhichIsA("ProximityPrompt")
				if body then
					table.insert(list, {obj = obj, body = body, prox = prox})
				end
			end
		end
		return list
	end
	
	-- Find closest heli
	local function getClosestHeli(heliList)
		local closest = nil
		local shortest = math.huge
		for _, heli in ipairs(heliList) do
			local dist = (hrp.Position - heli.body.Position).Magnitude
			if dist < shortest then
				shortest = dist
				closest = heli
			end
		end
		return closest
	end
	
	-- Main TP loop
	local function tpLoop()
		while runToggle do
			local heliList = findHelis()
	
			if #heliList > 0 then
				local heli = getClosestHeli(heliList)
	
				-- Notification when a heli_crash is found
				game.StarterGui:SetCore("SendNotification", {
					Title = "Heli Found",
					Text = "Found a HeliCrash!",
					Duration = 3
				})
	
				-- Loop teleport near closest heli
				while runToggle and heli.obj.Parent and workspace.interact:FindFirstChild(heli.obj.Name) do
					local targetPos = heli.body.Position + (heli.body.CFrame.LookVector * -2)
					hrp.CFrame = CFrame.new(targetPos, heli.body.Position)
	
					-- Camera looks at heli with short wait
					cam.CFrame = CFrame.new(cam.CFrame.Position, heli.body.Position)
					task.wait(0.1)
	
					if heli.prox then
						heli.prox.HoldDuration = 0
						heli.prox:InputHoldBegin()
						heli.prox:InputHoldEnd()
					end
	
					task.wait(0.05)
				end
			else
				-- Line/Grid TP
				hrp.CFrame = CFrame.new(getNextGridPos())
				task.wait(0.05)
			end
		end
	end
	
	-- Button toggle
	button.MouseButton1Click:Connect(function()
		runToggle = not runToggle
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
	
		if runToggle then
			button.Text = "STOP"
			if not oldPos then
				oldPos = hrp.CFrame
			end
			task.spawn(tpLoop)
		else
			button.Text = "FARM HELI-CRASH (LINE TP)"
			if oldPos then
				hrp.CFrame = oldPos
				oldPos = nil
			end
		end
	end)
end
coroutine.wrap(USYUWD_fake_script)()
local function WOUQIIG_fake_script() -- TextButton_46.LocalScript 
	local script = Instance.new('LocalScript', TextButton_46)

	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	local cam = workspace.CurrentCamera
	
	-- Circular TP settings
	local centerPos = Vector3.new(1325, 48, 1322) -- center of circle
	local radius = 50
	local yOffset = 12
	local angle = 0
	local angleStep = math.rad(15) -- 15 degrees per step
	local runToggle = false
	local oldPos = nil
	local button = script.Parent
	
	-- Generate next circular TP position
	local function getNextCircularPos()
		local x = centerPos.X + math.cos(angle) * radius
		local z = centerPos.Z + math.sin(angle) * radius
		local pos = Vector3.new(x, centerPos.Y + yOffset, z)
		angle += angleStep
		if angle >= math.rad(360) then
			angle = 0 -- reset after full circle
		end
		return pos
	end
	
	-- Find all heli_crash
	local function findHelis()
		local list = {}
		for _, obj in ipairs(workspace.interact:GetChildren()) do
			if obj.Name == "heli_crash" then
				local body = obj:FindFirstChild("Body")
				local prox = body and body:FindFirstChildWhichIsA("ProximityPrompt")
				if body then
					table.insert(list, {obj = obj, body = body, prox = prox})
				end
			end
		end
		return list
	end
	
	-- Find closest heli
	local function getClosestHeli(heliList)
		local closest = nil
		local shortest = math.huge
		for _, heli in ipairs(heliList) do
			local dist = (hrp.Position - heli.body.Position).Magnitude
			if dist < shortest then
				shortest = dist
				closest = heli
			end
		end
		return closest
	end
	
	-- Main TP loop
	local function tpLoop()
		while runToggle do
			local heliList = findHelis()
	
			if #heliList > 0 then
				local heli = getClosestHeli(heliList)
	
				-- Notification when a heli_crash is found
				game.StarterGui:SetCore("SendNotification", {
					Title = "Heli Found",
					Text = "Found a HeliCrash!",
					Duration = 3
				})
	
				-- Loop teleport near closest heli
				while runToggle and heli.obj.Parent and workspace.interact:FindFirstChild(heli.obj.Name) do
					local targetPos = heli.body.Position + (heli.body.CFrame.LookVector * -2)
					hrp.CFrame = CFrame.new(targetPos, heli.body.Position)
	
					-- Camera looks at heli with short wait
					cam.CFrame = CFrame.new(cam.CFrame.Position, heli.body.Position)
					task.wait(0.1)
	
					if heli.prox then
						heli.prox.HoldDuration = 0
						heli.prox:InputHoldBegin()
						heli.prox:InputHoldEnd()
					end
	
					task.wait(0.05)
				end
			else
				-- Circular TP
				hrp.CFrame = CFrame.new(getNextCircularPos())
				task.wait(0.05)
			end
		end
	end
	
	-- Button toggle
	button.MouseButton1Click:Connect(function()
		runToggle = not runToggle
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
	
		if runToggle then
			button.Text = "STOP"
			if not oldPos then
				oldPos = hrp.CFrame
			end
			task.spawn(tpLoop)
		else
			button.Text = "FARM HELI-CRASH (CIRCLE TP)"
			if oldPos then
				hrp.CFrame = oldPos
				oldPos = nil
			end
		end
	end)
end
coroutine.wrap(WOUQIIG_fake_script)()
local function NRDKCZ_fake_script() -- TextButton_47.LocalScript 
	local script = Instance.new('LocalScript', TextButton_47)

	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	local cam = workspace.CurrentCamera
	-- Spiral TP settings
	local centerPos = Vector3.new(1325, 48, 1322) -- updated center
	local radius = 5
	local radiusStep = 5
	local angle = 0
	local angleStep = math.rad(30) -- 30 degrees per step
	local yOffset = 12
	local runToggle = false
	local oldPos = nil
	local button = script.Parent
	
	-- Generate next spiral TP position
	local function getNextSpiralPos()
		local x = centerPos.X + math.cos(angle) * radius
		local z = centerPos.Z + math.sin(angle) * radius
		local pos = Vector3.new(x, centerPos.Y + yOffset, z)
		angle += angleStep
		if angle >= math.rad(360) then
			angle = 0
			radius = radius + radiusStep
		end
		return pos
	end
	
	-- Find all heli_crash
	local function findHelis()
		local list = {}
		for _, obj in ipairs(workspace.interact:GetChildren()) do
			if obj.Name == "heli_crash" then
				local body = obj:FindFirstChild("Body")
				local prox = body and body:FindFirstChildWhichIsA("ProximityPrompt")
				if body then
					table.insert(list, {obj = obj, body = body, prox = prox})
				end
			end
		end
		return list
	end
	
	-- Find closest heli
	local function getClosestHeli(heliList)
		local closest = nil
		local shortest = math.huge
		for _, heli in ipairs(heliList) do
			local dist = (hrp.Position - heli.body.Position).Magnitude
			if dist < shortest then
				shortest = dist
				closest = heli
			end
		end
		return closest
	end
	
	-- Main TP loop
	local function tpLoop()
		while runToggle do
			local heliList = findHelis()
	
			if #heliList > 0 then
				local heli = getClosestHeli(heliList)
	
				-- Notification when a heli_crash is found
				game.StarterGui:SetCore("SendNotification", {
					Title = "Heli Found",
					Text = "Found a HeliCrash!",
					Duration = 3
				})
	
				-- Loop teleport near closest heli
				while runToggle and heli.obj.Parent and workspace.interact:FindFirstChild(heli.obj.Name) do
					local targetPos = heli.body.Position + (heli.body.CFrame.LookVector * -2)
					hrp.CFrame = CFrame.new(targetPos, heli.body.Position)
	
					-- Camera looks at heli with short wait
					cam.CFrame = CFrame.new(cam.CFrame.Position, heli.body.Position)
					task.wait(0.1)
	
					if heli.prox then
						heli.prox.HoldDuration = 0
						heli.prox:InputHoldBegin()
						heli.prox:InputHoldEnd()
					end
	
					task.wait(0.05)
				end
			else
				-- Spiral TP
				hrp.CFrame = CFrame.new(getNextSpiralPos())
				task.wait(0.05)
			end
		end
	end
	
	-- Button toggle
	button.MouseButton1Click:Connect(function()
		runToggle = not runToggle
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
	
		if runToggle then
			button.Text = "STOP"
			if not oldPos then
				oldPos = hrp.CFrame
			end
			task.spawn(tpLoop)
		else
			button.Text = "FARM HELI-CRASH (SPIRAL TP)"
			if oldPos then
				hrp.CFrame = oldPos
				oldPos = nil
			end
		end
	end)
end
coroutine.wrap(NRDKCZ_fake_script)()
local function OOHQRCD_fake_script() -- TextButton_48.LocalScript 
	local script = Instance.new('LocalScript', TextButton_48)

	--// Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StarterGui = game:GetService("StarterGui")
	local UserInputService = game:GetService("UserInputService")
	
	--// Player
	local LocalPlayer = Players.LocalPlayer
	local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
	local Humanoid = Character:WaitForChild("Humanoid")
	
	--// Remotes
	local shootRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot")
	
	--// Toggle + storage
	local toggle = false
	local oldPos = nil
	local collectedGlocks = {}
	local tpToGlockActive = false
	
	-- Random TP positions
	local pos1 = Vector3.new(418, 27, 2102)
	local pos2 = Vector3.new(2155, 40, 542)
	local randomPositions = {pos1, pos2}
	
	---------------------------------------------------------------------
	--// Helpers
	local function notify(msg, dur)
		StarterGui:SetCore("SendNotification", {
			Title = "Script",
			Text = msg,
			Duration = dur or 1.7
		})
	end
	
	local function safeTeleportTo(pos)
		if HumanoidRootPart then
			HumanoidRootPart.Anchored = false
			HumanoidRootPart.CFrame = CFrame.new(pos)
		end
	end
	
	local function getNearestEnemy()
		local nearest, dist = nil, math.huge
		for _, enemy in pairs(workspace:WaitForChild("enemies"):GetChildren()) do
			if enemy:FindFirstChild("Head") and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
				local mag = (HumanoidRootPart.Position - enemy.Head.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = enemy
				end
			end
		end
		return nearest
	end
	
	--// Shoot enemy (reversed remote)
	local function shootEnemy(enemy)
		if not (enemy and enemy:FindFirstChild("Head") and enemy:FindFirstChild("HumanoidRootPart")) then return end
		local args = {
			enemy.HumanoidRootPart.CFrame,
			HumanoidRootPart.CFrame
		}
		shootRemote:FireServer(unpack(args))
	end
	
	--// Auto equip tool that has a child "ammo"
	local function autoEquipAmmo()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if not backpack then return nil end
		for _, tool in pairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool:FindFirstChild("ammo") then
				Humanoid:EquipTool(tool)
				return tool
			end
		end
		return nil
	end
	
	--// Get nearest Glock
	local function getNearestGlock()
		local nearest, dist = nil, math.huge
		for _, g in pairs(workspace:WaitForChild("dropped"):GetChildren()) do
			if (g.Name == "glock" or g.Name == "Glock") and g:FindFirstChild("Handle") and not collectedGlocks[g] then
				local mag = (HumanoidRootPart.Position - g.Handle.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = g
				end
			end
		end
		return nearest
	end
	
	--// Auto pickup nearest Glock using HoldDuration + InputHold + Triggered
	local function autoPickupGlock()
		local glock = getNearestGlock()
		if glock and glock:FindFirstChild("Handle") then
			local prox = glock.Handle:FindFirstChildOfClass("ProximityPrompt")
			if not prox then return end
	
			prox.HoldDuration = 0 -- instant pickup
			local cam = workspace.CurrentCamera
			local collected = false
	
			-- Detect actual pickup
			local connection = prox.Triggered:Connect(function()
				collected = true
			end)
	
			-- Loop: TP to Glock, camera look, simulate hold input
			while glock.Parent and toggle and not collected do
				safeTeleportTo(glock.Handle.Position + Vector3.new(0, 1.2, 0))
				cam.CFrame = CFrame.new(cam.CFrame.Position, glock.Handle.Position)
	
				-- Simulate holding the prompt
				prox:InputHoldBegin(Enum.UserInputType.Keyboard)
				task.wait(0.05)
				prox:InputHoldEnd(Enum.UserInputType.Keyboard)
	
				task.wait(0.1)
			end
	
			connection:Disconnect()
	
			if collected then
				collectedGlocks[glock] = true
				notify("Collected Glock!", 1.7)
			end
		end
	end
	
	--// Random TP (can teleport to same position multiple times)
	local function randomTP()
		if #randomPositions == 0 then return end
		local index = math.random(1, #randomPositions)
		local pos = randomPositions[index]
		safeTeleportTo(Vector3.new(pos.X, oldPos.Y + 12, pos.Z))
	end
	
	---------------------------------------------------------------------
	--// Main loop
	local function mainLoop()
		oldPos = HumanoidRootPart.CFrame
		toggle = true
	
		task.spawn(function()
			while toggle do
				local enemy = getNearestEnemy()
				local nearestGlock = getNearestGlock()
	
				if enemy then
					autoEquipAmmo()
					safeTeleportTo(enemy.Head.Position + Vector3.new(0, -9, 0))
					shootEnemy(enemy)
	
				elseif nearestGlock then
					tpToGlockActive = true
					autoPickupGlock()
					tpToGlockActive = false
					task.wait(0.1)
	
				else
					if not tpToGlockActive then
						randomTP()
						task.wait(0.1)
					end
				end
	
				task.wait()
			end
		end)
	end
	
	--// Stop loop
	local function stopLoop()
		toggle = false
		if oldPos then
			safeTeleportTo(oldPos.Position)
		end
	end
	
	---------------------------------------------------------------------
	--// GUI Button
	local Button = script.Parent
	
	local on = false
	Button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			mainLoop()
			Button.Text = "STOP"
		else
			stopLoop()
			Button.Text = "FARM GLOCK (RANDOM TP)"
		end
	end)
	
	---------------------------------------------------------------------
	--// Death Notification with Glock count
	LocalPlayer.CharacterAdded:Connect(function(char)
		local hum = char:WaitForChild("Humanoid")
		hum.Died:Connect(function()
			notify("You died! Collected Glocks: ".. #collectedGlocks, 3)
		end)
	end)
end
coroutine.wrap(OOHQRCD_fake_script)()
local function HLZS_fake_script() -- TextButton_49.LocalScript 
	local script = Instance.new('LocalScript', TextButton_49)

	--// Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StarterGui = game:GetService("StarterGui")
	local UserInputService = game:GetService("UserInputService")
	
	--// Player
	local LocalPlayer = Players.LocalPlayer
	local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
	local Humanoid = Character:WaitForChild("Humanoid")
	
	--// Remotes
	local shootRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot")
	
	--// Toggle + storage
	local toggle = false
	local oldPos = nil
	local collectedGlocks = {}
	local tpToGlockActive = false
	
	-- Random TP positions
	local pos1 = Vector3.new(418, 27, 2102)
	local pos2 = Vector3.new(2155, 40, 542)
	local randomPositions = {pos1, pos2}
	
	---------------------------------------------------------------------
	--// Helpers
	local function notify(msg, dur)
		StarterGui:SetCore("SendNotification", {
			Title = "Script",
			Text = msg,
			Duration = dur or 1.7
		})
	end
	
	--// Smooth Line TP
	local function lineTeleportTo(targetPos, speed)
		speed = speed or 150 -- studs/sec
		if not HumanoidRootPart then return end
	
		local startPos = HumanoidRootPart.Position
		local distance = (targetPos - startPos).Magnitude
		if distance == 0 then return end
	
		local direction = (targetPos - startPos).Unit
		local travelTime = distance / speed
		local elapsed = 0
		local step = 0.03
	
		while elapsed < travelTime and toggle do
			local newPos = startPos + direction * (elapsed / travelTime * distance)
			HumanoidRootPart.CFrame = CFrame.new(newPos)
			task.wait(step)
			elapsed = elapsed + step
		end
		HumanoidRootPart.CFrame = CFrame.new(targetPos)
	end
	
	--// Get nearest enemy
	local function getNearestEnemy()
		local nearest, dist = nil, math.huge
		for _, enemy in pairs(workspace:WaitForChild("enemies"):GetChildren()) do
			if enemy:FindFirstChild("Head") and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
				local mag = (HumanoidRootPart.Position - enemy.Head.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = enemy
				end
			end
		end
		return nearest
	end
	
	--// Shoot enemy
	local function shootEnemy(enemy)
		if not (enemy and enemy:FindFirstChild("Head") and enemy:FindFirstChild("HumanoidRootPart")) then return end
		local args = {
			enemy.HumanoidRootPart.CFrame,
			HumanoidRootPart.CFrame
		}
		shootRemote:FireServer(unpack(args))
	end
	
	--// Auto equip tool with ammo
	local function autoEquipAmmo()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if not backpack then return nil end
		for _, tool in pairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool:FindFirstChild("ammo") then
				Humanoid:EquipTool(tool)
				return tool
			end
		end
		return nil
	end
	
	--// Get nearest Glock
	local function getNearestGlock()
		local nearest, dist = nil, math.huge
		for _, g in pairs(workspace:WaitForChild("dropped"):GetChildren()) do
			if (g.Name == "glock" or g.Name == "Glock") and g:FindFirstChild("Handle") and not collectedGlocks[g] then
				local mag = (HumanoidRootPart.Position - g.Handle.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = g
				end
			end
		end
		return nearest
	end
	
	--// Auto pickup nearest Glock
	local function autoPickupGlock()
		local glock = getNearestGlock()
		if glock and glock:FindFirstChild("Handle") then
			local prox = glock.Handle:FindFirstChildOfClass("ProximityPrompt")
			if not prox then return end
	
			prox.HoldDuration = 0 -- instant pickup
			local cam = workspace.CurrentCamera
			local collected = false
	
			-- Detect actual pickup
			local connection = prox.Triggered:Connect(function()
				collected = true
			end)
	
			-- Loop: TP to Glock, camera look, simulate hold input
			while glock.Parent and toggle and not collected do
				lineTeleportTo(glock.Handle.Position + Vector3.new(0, 1.2, 0), 250)
				cam.CFrame = CFrame.new(cam.CFrame.Position, glock.Handle.Position)
	
				-- Simulate holding the prompt
				prox:InputHoldBegin(Enum.UserInputType.Keyboard)
				task.wait(0.05)
				prox:InputHoldEnd(Enum.UserInputType.Keyboard)
	
				task.wait(0.05)
			end
	
			connection:Disconnect()
	
			if collected then
				collectedGlocks[glock] = true
				notify("Collected Glock!", 1.7)
			end
		end
	end
	
	--// Random TP
	local function randomTP()
		if #randomPositions == 0 then return end
		local index = math.random(1, #randomPositions)
		local pos = randomPositions[index]
		lineTeleportTo(Vector3.new(pos.X, oldPos.Y + 12, pos.Z), 180)
	end
	
	---------------------------------------------------------------------
	--// Main loop
	local function mainLoop()
		oldPos = HumanoidRootPart.CFrame
		toggle = true
	
		task.spawn(function()
			while toggle do
				local enemy = getNearestEnemy()
				local nearestGlock = getNearestGlock()
	
				if enemy then
					autoEquipAmmo()
					lineTeleportTo(enemy.Head.Position + Vector3.new(0, -9, 0), 200)
					shootEnemy(enemy)
	
				elseif nearestGlock then
					tpToGlockActive = true
					autoPickupGlock()
					tpToGlockActive = false
					task.wait(0.1)
	
				else
					if not tpToGlockActive then
						randomTP()
						task.wait(0.1)
					end
				end
	
				task.wait()
			end
		end)
	end
	
	--// Stop loop
	local function stopLoop()
		toggle = false
		if oldPos then
			lineTeleportTo(oldPos.Position, 200)
		end
	end
	
	---------------------------------------------------------------------
	--// GUI Button
	local Button = script.Parent
	
	local on = false
	Button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			mainLoop()
			Button.Text = "STOP"
		else
			stopLoop()
			Button.Text = "FARM GLOCK (LINE TP)"
		end
	end)
	
	---------------------------------------------------------------------
	--// Death Notification with Glock count
	LocalPlayer.CharacterAdded:Connect(function(char)
		local hum = char:WaitForChild("Humanoid")
		hum.Died:Connect(function()
			notify("You died! Collected Glocks: ".. #collectedGlocks, 3)
		end)
	end)
end
coroutine.wrap(HLZS_fake_script)()
local function VFMFZ_fake_script() -- TextButton_50.LocalScript 
	local script = Instance.new('LocalScript', TextButton_50)

	--// Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StarterGui = game:GetService("StarterGui")
	local UserInputService = game:GetService("UserInputService")
	
	--// Player
	local LocalPlayer = Players.LocalPlayer
	local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
	local Humanoid = Character:WaitForChild("Humanoid")
	
	--// Remotes
	local shootRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot")
	
	--// Toggle + storage
	local toggle = false
	local oldPos = nil
	local collectedGlocks = {}
	local tpToGlockActive = false
	
	-- Circle TP center
	local centerPos = Vector3.new(1325, 48, 1322)
	local circleRadius = 20
	local circleSpeed = 2 -- radians per second
	
	---------------------------------------------------------------------
	--// Helpers
	local function notify(msg, dur)
		StarterGui:SetCore("SendNotification", {
			Title = "Script",
			Text = msg,
			Duration = dur or 1.7
		})
	end
	
	--// Smooth Line TP
	local function lineTeleportTo(targetPos, speed)
		speed = speed or 150 -- studs/sec
		if not HumanoidRootPart then return end
	
		local startPos = HumanoidRootPart.Position
		local distance = (targetPos - startPos).Magnitude
		if distance == 0 then return end
	
		local direction = (targetPos - startPos).Unit
		local travelTime = distance / speed
		local elapsed = 0
		local step = 0.03
	
		while elapsed < travelTime and toggle do
			local newPos = startPos + direction * (elapsed / travelTime * distance)
			HumanoidRootPart.CFrame = CFrame.new(newPos)
			task.wait(step)
			elapsed = elapsed + step
		end
		HumanoidRootPart.CFrame = CFrame.new(targetPos)
	end
	
	--// Circular TP
	local function circularTP(duration)
		duration = duration or 10 -- seconds
		local elapsed = 0
		local step = 0.03
		local cam = workspace.CurrentCamera
	
		while elapsed < duration and toggle do
			local angle = elapsed * circleSpeed
			local x = centerPos.X + math.cos(angle) * circleRadius
			local z = centerPos.Z + math.sin(angle) * circleRadius
			local y = centerPos.Y
			local newPos = Vector3.new(x, y, z)
	
			HumanoidRootPart.CFrame = CFrame.new(newPos)
			cam.CFrame = CFrame.new(cam.CFrame.Position, centerPos)
	
			task.wait(step)
			elapsed = elapsed + step
		end
	end
	
	--// Get nearest enemy
	local function getNearestEnemy()
		local nearest, dist = nil, math.huge
		for _, enemy in pairs(workspace:WaitForChild("enemies"):GetChildren()) do
			if enemy:FindFirstChild("Head") and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
				local mag = (HumanoidRootPart.Position - enemy.Head.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = enemy
				end
			end
		end
		return nearest
	end
	
	--// Shoot enemy
	local function shootEnemy(enemy)
		if not (enemy and enemy:FindFirstChild("Head") and enemy:FindFirstChild("HumanoidRootPart")) then return end
		local args = {
			enemy.HumanoidRootPart.CFrame,
			HumanoidRootPart.CFrame
		}
		shootRemote:FireServer(unpack(args))
	end
	
	--// Auto equip tool with ammo
	local function autoEquipAmmo()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if not backpack then return nil end
		for _, tool in pairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool:FindFirstChild("ammo") then
				Humanoid:EquipTool(tool)
				return tool
			end
		end
		return nil
	end
	
	--// Get nearest Glock
	local function getNearestGlock()
		local nearest, dist = nil, math.huge
		for _, g in pairs(workspace:WaitForChild("dropped"):GetChildren()) do
			if (g.Name == "glock" or g.Name == "Glock") and g:FindFirstChild("Handle") and not collectedGlocks[g] then
				local mag = (HumanoidRootPart.Position - g.Handle.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = g
				end
			end
		end
		return nearest
	end
	
	--// Auto pickup nearest Glock
	local function autoPickupGlock()
		local glock = getNearestGlock()
		if glock and glock:FindFirstChild("Handle") then
			local prox = glock.Handle:FindFirstChildOfClass("ProximityPrompt")
			if not prox then return end
	
			prox.HoldDuration = 0 -- instant pickup
			local cam = workspace.CurrentCamera
			local collected = false
	
			-- Detect actual pickup
			local connection = prox.Triggered:Connect(function()
				collected = true
			end)
	
			-- Loop: TP to Glock, camera look, simulate hold input
			while glock.Parent and toggle and not collected do
				lineTeleportTo(glock.Handle.Position + Vector3.new(0, 1.2, 0), 250)
				cam.CFrame = CFrame.new(cam.CFrame.Position, glock.Handle.Position)
	
				-- Simulate holding the prompt
				prox:InputHoldBegin(Enum.UserInputType.Keyboard)
				task.wait(0.05)
				prox:InputHoldEnd(Enum.UserInputType.Keyboard)
	
				task.wait(0.05)
			end
	
			connection:Disconnect()
	
			if collected then
				collectedGlocks[glock] = true
				notify("Collected Glock!", 1.7)
			end
		end
	end
	
	---------------------------------------------------------------------
	--// Main loop
	local function mainLoop()
		oldPos = HumanoidRootPart.CFrame
		toggle = true
	
		task.spawn(function()
			while toggle do
				local enemy = getNearestEnemy()
				local nearestGlock = getNearestGlock()
	
				if enemy then
					autoEquipAmmo()
					lineTeleportTo(enemy.Head.Position + Vector3.new(0, -9, 0), 200)
					shootEnemy(enemy)
	
				elseif nearestGlock then
					tpToGlockActive = true
					autoPickupGlock()
					tpToGlockActive = false
					task.wait(0.1)
	
				else
					if not tpToGlockActive then
						circularTP(5) -- orbit around center for 5 seconds
						task.wait(0.1)
					end
				end
	
				task.wait()
			end
		end)
	end
	
	--// Stop loop
	local function stopLoop()
		toggle = false
		if oldPos then
			lineTeleportTo(oldPos.Position, 200)
		end
	end
	
	---------------------------------------------------------------------
	--// GUI Button
	local Button = script.Parent
	
	local on = false
	Button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			mainLoop()
			Button.Text = "STOP"
		else
			stopLoop()
			Button.Text = "FARM GLOCK (CIRCLE TP)"
		end
	end)
	
	---------------------------------------------------------------------
	--// Death Notification with Glock count
	LocalPlayer.CharacterAdded:Connect(function(char)
		local hum = char:WaitForChild("Humanoid")
		hum.Died:Connect(function()
			notify("You died! Collected Glocks: ".. #collectedGlocks, 3)
		end)
	end)
	
end
coroutine.wrap(VFMFZ_fake_script)()
local function NUQQZSH_fake_script() -- TextButton_51.LocalScript 
	local script = Instance.new('LocalScript', TextButton_51)

	--// Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StarterGui = game:GetService("StarterGui")
	local UserInputService = game:GetService("UserInputService")
	
	--// Player
	local LocalPlayer = Players.LocalPlayer
	local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
	local Humanoid = Character:WaitForChild("Humanoid")
	
	--// Remotes
	local shootRemote = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot")
	
	--// Toggle + storage
	local toggle = false
	local oldPos = nil
	local collectedGlocks = {}
	local tpToGlockActive = false
	
	-- Spiral TP center
	local centerPos = Vector3.new(1325, 48, 1322)
	local spiralRadius = 20
	local spiralSpeed = 2 -- radians/sec
	local verticalSpeed = 1 -- studs/sec
	
	---------------------------------------------------------------------
	--// Helpers
	local function notify(msg, dur)
		StarterGui:SetCore("SendNotification", {
			Title = "Script",
			Text = msg,
			Duration = dur or 1.7
		})
	end
	
	--// Smooth Line TP
	local function lineTeleportTo(targetPos, speed)
		speed = speed or 150 -- studs/sec
		if not HumanoidRootPart then return end
	
		local startPos = HumanoidRootPart.Position
		local distance = (targetPos - startPos).Magnitude
		if distance == 0 then return end
	
		local direction = (targetPos - startPos).Unit
		local travelTime = distance / speed
		local elapsed = 0
		local step = 0.03
	
		while elapsed < travelTime and toggle do
			local newPos = startPos + direction * (elapsed / travelTime * distance)
			HumanoidRootPart.CFrame = CFrame.new(newPos)
			task.wait(step)
			elapsed = elapsed + step
		end
		HumanoidRootPart.CFrame = CFrame.new(targetPos)
	end
	
	--// Spiral TP around center
	local function spiralTP(duration)
		duration = duration or 10 -- seconds
		local elapsed = 0
		local step = 0.03
		local cam = workspace.CurrentCamera
	
		while elapsed < duration and toggle do
			local angle = elapsed * spiralSpeed
			local x = centerPos.X + math.cos(angle) * spiralRadius
			local z = centerPos.Z + math.sin(angle) * spiralRadius
			local y = centerPos.Y + verticalSpeed * elapsed
			local newPos = Vector3.new(x, y, z)
	
			HumanoidRootPart.CFrame = CFrame.new(newPos)
			cam.CFrame = CFrame.new(cam.CFrame.Position, centerPos)
	
			task.wait(step)
			elapsed = elapsed + step
		end
	end
	
	--// Get nearest enemy
	local function getNearestEnemy()
		local nearest, dist = nil, math.huge
		for _, enemy in pairs(workspace:WaitForChild("enemies"):GetChildren()) do
			if enemy:FindFirstChild("Head") and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
				local mag = (HumanoidRootPart.Position - enemy.Head.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = enemy
				end
			end
		end
		return nearest
	end
	
	--// Shoot enemy
	local function shootEnemy(enemy)
		if not (enemy and enemy:FindFirstChild("Head") and enemy:FindFirstChild("HumanoidRootPart")) then return end
		local args = {
			enemy.HumanoidRootPart.CFrame,
			HumanoidRootPart.CFrame
		}
		shootRemote:FireServer(unpack(args))
	end
	
	--// Auto equip tool with ammo
	local function autoEquipAmmo()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if not backpack then return nil end
		for _, tool in pairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool:FindFirstChild("ammo") then
				Humanoid:EquipTool(tool)
				return tool
			end
		end
		return nil
	end
	
	--// Get nearest Glock
	local function getNearestGlock()
		local nearest, dist = nil, math.huge
		for _, g in pairs(workspace:WaitForChild("dropped"):GetChildren()) do
			if (g.Name == "glock" or g.Name == "Glock") and g:FindFirstChild("Handle") and not collectedGlocks[g] then
				local mag = (HumanoidRootPart.Position - g.Handle.Position).Magnitude
				if mag < dist then
					dist = mag
					nearest = g
				end
			end
		end
		return nearest
	end
	
	--// Auto pickup nearest Glock
	local function autoPickupGlock()
		local glock = getNearestGlock()
		if glock and glock:FindFirstChild("Handle") then
			local prox = glock.Handle:FindFirstChildOfClass("ProximityPrompt")
			if not prox then return end
	
			prox.HoldDuration = 0 -- instant pickup
			local cam = workspace.CurrentCamera
			local collected = false
	
			-- Detect actual pickup
			local connection = prox.Triggered:Connect(function()
				collected = true
			end)
	
			-- Loop: TP to Glock, camera look, simulate hold input
			while glock.Parent and toggle and not collected do
				lineTeleportTo(glock.Handle.Position + Vector3.new(0, 1.2, 0), 250)
				cam.CFrame = CFrame.new(cam.CFrame.Position, glock.Handle.Position)
	
				-- Simulate holding the prompt
				prox:InputHoldBegin(Enum.UserInputType.Keyboard)
				task.wait(0.05)
				prox:InputHoldEnd(Enum.UserInputType.Keyboard)
	
				task.wait(0.05)
			end
	
			connection:Disconnect()
	
			if collected then
				collectedGlocks[glock] = true
				notify("Collected Glock!", 1.7)
			end
		end
	end
	
	---------------------------------------------------------------------
	--// Main loop
	local function mainLoop()
		oldPos = HumanoidRootPart.CFrame
		toggle = true
	
		task.spawn(function()
			while toggle do
				local enemy = getNearestEnemy()
				local nearestGlock = getNearestGlock()
	
				if enemy then
					autoEquipAmmo()
					lineTeleportTo(enemy.Head.Position + Vector3.new(0, -9, 0), 200)
					shootEnemy(enemy)
	
				elseif nearestGlock then
					tpToGlockActive = true
					autoPickupGlock()
					tpToGlockActive = false
					task.wait(0.1)
	
				else
					if not tpToGlockActive then
						spiralTP(5) -- orbit and rise around center for 5 seconds
						task.wait(0.1)
					end
				end
	
				task.wait()
			end
		end)
	end
	
	--// Stop loop
	local function stopLoop()
		toggle = false
		if oldPos then
			lineTeleportTo(oldPos.Position, 200)
		end
	end
	
	---------------------------------------------------------------------
	--// GUI Button
	local Button = script.Parent
	
	local on = false
	Button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			mainLoop()
			Button.Text = "STOP"
		else
			stopLoop()
			Button.Text = "FARM GLOCK (SPIRAL TP)"
		end
	end)
	
	---------------------------------------------------------------------
	--// Death Notification with Glock count
	LocalPlayer.CharacterAdded:Connect(function(char)
		local hum = char:WaitForChild("Humanoid")
		hum.Died:Connect(function()
			notify("You died! Collected Glocks: ".. #collectedGlocks, 3)
		end)
	end)
end
coroutine.wrap(NUQQZSH_fake_script)()

	local TextBox = script.Parent.TextBox
	local Button = script.Parent
	
	local isActive = false
	local loopDelay = .1
	
	-- Find nearest animal within max distance
	local function getNearestAnimal(maxDistance)
		local nearest = nil
		local nearestDist = maxDistance
		for _, animal in pairs(workspace:WaitForChild("scps"):GetChildren()) do
			local animalHrp = animal:FindFirstChild("HumanoidRootPart")
			local animalHead = animal:FindFirstChild("Head")
			if animalHrp and animalHead then
				local dist = (Hrp.Position - animalHrp.Position).Magnitude
				if dist <= nearestDist then
					nearest = animal
					nearestDist = dist
				end
			end
		end
		return nearest
	end
	
	-- Function to start shooting loop
	local function startShooting()
		spawn(function()
			while isActive do
				local maxDist = tonumber(TextBox.Text)
				if maxDist then
					local nearestAnimal = getNearestAnimal(maxDist)
					if nearestAnimal and nearestAnimal:FindFirstChild("HumanoidRootPart") and nearestAnimal:FindFirstChild("Head") then
						local args = {
							nearestAnimal.HumanoidRootPart.CFrame,
							nearestAnimal.Head.CFrame
						}
						ReplicatedStorage:WaitForChild("remotes"):WaitForChild("shoot"):FireServer(unpack(args))
					end
				else
					warn("Enter a valid number for max distance!")
				end
				task.wait(loopDelay)
			end
		end)
	end
	
	-- Toggle button
	Button.MouseButton1Click:Connect(function()
		isActive = not isActive
		if isActive then
			startShooting()
		end
	end)
	
end
coroutine.wrap(ZAOL_fake_script)()
local function HKJAINP_fake_script() -- TextButton_13.LocalScript 
	local script = Instance.new('LocalScript', TextButton_13)

	local player = game.Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	local RunService = game:GetService("RunService")
	local button = script.Parent
	
	-- References
	local backpack = player:WaitForChild("Backpack")
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	-- States
	local farming = false
	local shootConnection
	local autoEquipConnection
	
	-- Function to equip all ammo using Humanoid:EquipTool()
	local function equipAllAmmo()
		-- Collect already equipped ammo (tools currently in character)
		local equipped = {}
		for _, tool in pairs(character:GetChildren()) do
			if tool:IsA("Tool") then
				equipped[tool] = true
			end
		end
	
		-- Check all items in Backpack and Character
		local function checkContainer(container)
			for _, item in pairs(container:GetDescendants()) do
				if item:IsA("Tool") then
					local ammoValue = item:FindFirstChild("ammo")
					if ammoValue and ammoValue:IsA("NumberValue") and not equipped[item] then
						humanoid:EquipTool(item) -- equip via Humanoid
						equipped[item] = true
					end
				end
			end
		end
	
		checkContainer(backpack)
		checkContainer(character)
	end
	
	-- Button click
	button.MouseButton1Click:Connect(function()
		farming = not farming
		if farming then
			button.Text = "STOP INSTANT KILL MAMMOTH"
	
			-- Equip all ammo immediately
			equipAllAmmo()
	
			-- Listen for new items added in Backpack
			autoEquipConnection = backpack.DescendantAdded:Connect(function(item)
				if item:IsA("Tool") then
					local ammoValue = item:FindFirstChild("ammo")
					if ammoValue and ammoValue:IsA("NumberValue") then
						humanoid:EquipTool(item)
					end
				end
			end)
	
			-- Auto shoot mammoths
			shootConnection = RunService.Heartbeat:Connect(function()
				local animals = workspace:WaitForChild("animals")
	
				for _, animal in pairs(animals:GetChildren()) do
					if animal.Name == "mammoth" then
						local mainPart = animal:FindFirstChild("Main") or animal:FindFirstChild("main")
						local hrp = animal:FindFirstChild("HumanoidRootPart")
						local animalHumanoid = animal:FindFirstChild("Humanoid")
	
						if mainPart and hrp and animalHumanoid and animalHumanoid.Health > 0 then
							local args = { mainPart.CFrame, hrp.CFrame }
							local shootRemote = game:GetService("ReplicatedStorage")
								:WaitForChild("remotes")
								:WaitForChild("shoot")
	
							-- Fire 23 times instantly per Heartbeat
							for i = 1, 23 do
								shootRemote:FireServer(unpack(args))
							end
						end
					end
				end
			end)
	
		else
			button.Text = "INSTANT KILL MAMMOTH"
	
			-- Stop shooting
			if shootConnection then
				shootConnection:Disconnect()
				shootConnection = nil
			end
	
			-- Stop auto-equip
			if autoEquipConnection then
				autoEquipConnection:Disconnect()
				autoEquipConnection = nil
			end
		end
	end)
end
coroutine.wrap(HKJAINP_fake_script)()
local function CVWW_fake_script() -- TextButton_14.LocalScript 
	local script = Instance.new('LocalScript', TextButton_14)

	-- Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
	local player = Players.LocalPlayer
	local remotes = ReplicatedStorage:WaitForChild("remotes")
	local shootRemote = remotes:WaitForChild("shoot")
	
	local Button = script.Parent
	
	-- Toggle state
	local shooting = false
	
	-- Function: get args where camera is looking
	local function getLookArgs()
		local cam = workspace.CurrentCamera
		if not cam then return nil end
	
		local origin = cam.CFrame
		-- Create a point far in front of camera
		local target = cam.CFrame + (cam.CFrame.LookVector * 1000)
	
		return {
			origin,
			target
		}
	end
	
	-- Loop
	local function shootLoop()
		while shooting do
			local args = getLookArgs()
			if args then
				shootRemote:FireServer(unpack(args))
			end
			task.wait(.1) -- fire rate
		end
	end
	
	-- Button toggle
	Button.MouseButton1Click:Connect(function()
		shooting = not shooting
		Button.Text = shooting and "STOP AUTO SHOOT" or "AUTO SHOOT"
		if shooting then
			task.spawn(shootLoop)
		end
	end)
end
coroutine.wrap(CVWW_fake_script)()
local function ELLO_fake_script() -- TextButton_15.LocalScript 
	local script = Instance.new('LocalScript', TextButton_15)

	local hrpPos = nil
	local StarterGui = game:GetService("StarterGui")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local cast = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("cast")
	local fishGUI = game.Players.LocalPlayer.PlayerGui:FindFirstChild("fish_system")
	local autoFishLoop
	
	local function findFishingRod()
		-- Check Backpack
		local rod = game.Players.LocalPlayer.Backpack:FindFirstChild("Fishing Rod")
		if rod then return rod end
	
		-- Check Character
		if game.Players.LocalPlayer.Character then
			rod = game.Players.LocalPlayer.Character:FindFirstChild("Fishing Rod")
			if rod then return rod end
		end
	
		return nil
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
			hrpPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			StarterGui:SetCore("SendNotification", {
				Title = "Succes",
				Text = "HumanoidRootPart position stored:"..hrpPos,
				Duration = 1
			})
		else
			StarterGui:SetCore("SendNotification", {
				Title = "Error",
				Text = "You don't have a character!",
				Duration = 1
			})
		end
	end)
	
	script.Parent.Parent.fish.MouseButton1Click:Connect(function()
		if not findFishingRod() then
			StarterGui:SetCore("SendNotification", {
				Title = "Error",
				Text = "No Fishing Rod found in Backpack or Character.",
				Duration = 1
			})
			return
		end
	
		if hrpPos then
			cast:FireServer(hrpPos)
			StarterGui:SetCore("SendNotification", {
				Title = "Succes",
				Text = "Remote fired at HumanoidRootPart position:", hrpPos,
				Duration = 1
			})
		else
			StarterGui:SetCore("SendNotification", {
				Title = "Error",
				Text = "You don't have a set a position!",
				Duration = 3
			})
		end
	end)
	
	script.Parent.Parent.FastFish.MouseButton1Click:Connect(function()
		if not findFishingRod() then
			StarterGui:SetCore("SendNotification", {
				Title = "Error",
				Text = "No Fishing Rod found in Backpack or Character.",
				Duration = 1
			})
			return
		end
	
		if hrpPos then
			cast:FireServer(hrpPos)
			task.wait(.1)
			fishGUI.Enable = true
			StarterGui:SetCore("SendNotification", {
				Title = "Succes",
				Text = "Remote fired at HumanoidRootPart posiiton:", hrpPos,
				Duration = 1
			})
		else
			StarterGui:SetCore("SendNotification", {
				Title = "Error",
				Text = "You don't have a set a position!",
				Duration = 3
			})
		end
	end)
	
	script.Parent.Parent.autoFish.MouseButton1Click:Connect(function()
		if autoFishLoop then
			autoFishLoop = nil
			script.Parent.Parent.autoFish.Text = "AUTO FISH"
		else
			if not hrpPos then
				StarterGui:SetCore("SendNotification", {
					Title = "ERROR",
					Text = "NO HUMANOIDROOTPART SET!",
					Duration = 1
				})
				return
			end
			autoFishLoop = true
			script.Parent.Parent.autoFish.Text = "STOP AUTO FISH"
	
			task.spawn(function()
				while autoFishLoop do
					-- FailSafe
					local char = game.Players.LocalPlayer.Character
					if not char or not char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Humanoid").Health <= 0 then
						autoFishLoop = nil
						StarterGui:SetCore("SendNotification", {
							Title = "ERROR",
							Text = "YOU DIED",
							Duration = 1
						})
						break
					end
	
					-- Rod check
					local rod = char:FindFirstChild("Fishing Rod")
					if not rod then
						rod = game.Players.LocalPlayer.Backpack:FindFirstChild("Fishing Rod")
						if rod then
							char.Humanoid:EquipTool(rod)
							StarterGui:SetCore("SendNotification", {
								Title = "SUCCES",
								Text = "FISHING ROD EQUIPED",
								Duration = 1
							})
						else
							autoFishLoop = nil
							StarterGui:SetCore("SendNotification", {
								Title = "ERROR",
								Text = "NO FISHING ROD",
								Duration = 1
							})
							break
						end
					end
	
					-- Cast
					cast:FireServer(hrpPos)
					task.wait(0.15)
	
					-- Enable fish_system GUI if exists
					local fishGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("fish_system")
					if fishGui then fishGui.Enabled = true end
					task.wait(0.1)
				end
			end)
		end
	end)
end
coroutine.wrap(ELLO_fake_script)()
local function CKGG_fake_script() -- autoFish.LocalScript 
	local script = Instance.new('LocalScript', autoFish)

	--// Services
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = Players.LocalPlayer
	local cast = ReplicatedStorage:WaitForChild("remotes"):WaitForChild("cast")
	local StarterGui = game:GetService("StarterGui")
	
	--// Vars
	local hrpPos = nil
	local pointerLoop
	
	--// Pointer Align Button
	local pointerBtn = script.Parent.Parent.AutoPointer
	pointerBtn.MouseButton1Click:Connect(function()
		if pointerLoop then
			pointerLoop = nil
			pointerBtn.Text = "AUTO FISH GAME"
		else
			pointerLoop = true
			pointerBtn.Text = "STOP AUTO FISH GAME"
			task.spawn(function()
				while pointerLoop do
					local fishGui = player.PlayerGui:FindFirstChild("fish_system")
					if fishGui and fishGui:FindFirstChild("game") then
						local gameFrame = fishGui.game
						if gameFrame:FindFirstChild("pointer") and gameFrame:FindFirstChild("fish") then
							gameFrame.pointer.Position = gameFrame.fish.Position
						end
					end
					task.wait()
				end
			end)
		end
	end)
	
end
coroutine.wrap(CKGG_fake_script)()
local function GCQPZ_fake_script() -- TextButton_16.LocalScript 
	local script = Instance.new('LocalScript', TextButton_16)

	local button = script.Parent
	local toggleEsp = false
	
	-- Function to create ESP for a single SCP
	local function createEsp(scp)
		if scp:FindFirstChild("Highlight") or scp:FindFirstChild("BillboardGui") then return end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Parent = scp
		highlight.Adornee = scp
		highlight.FillTransparency = 1
		highlight.OutlineTransparency = 0
		highlight.OutlineColor = Color3.new(1, 0, 0)
	
		-- BillboardGui
		local billboard = Instance.new("BillboardGui")
		billboard.Parent = scp
		billboard.Adornee = scp
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.AlwaysOnTop = true
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Parent = billboard
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = scp.Name
		textLabel.TextColor3 = Color3.new(1, 0, 0)
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggleEsp = not toggleEsp
	
		if toggleEsp then
			for _, scp in pairs(workspace.scps:GetChildren()) do
				createEsp(scp)
			end
		else
			for _, scp in pairs(workspace.scps:GetChildren()) do
				for _, child in pairs(scp:GetChildren()) do
					if child:IsA("Highlight") or child:IsA("BillboardGui") then
						child:Destroy()
					end
				end
			end
		end
	end)
	
	-- Add ESP automatically when new SCPs spawn
	workspace.scps.ChildAdded:Connect(function(child)
		if toggleEsp then
			createEsp(child)
		end
	end)
end
coroutine.wrap(GCQPZ_fake_script)()
local function IHDFA_fake_script() -- TextButton_17.LocalScript 
	local script = Instance.new('LocalScript', TextButton_17)

	local button = script.Parent
	local toggleEsp = false
	
	-- Function to create ESP for a single animal
	local function createAnimalEsp(animal)
		if animal.Name ~= "deer" then return end -- skip if not deer
		if animal:FindFirstChild("Highlight") or animal:FindFirstChild("BillboardGui") then return end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Parent = animal
		highlight.Adornee = animal
		highlight.FillTransparency = 1
		highlight.OutlineTransparency = 0
		highlight.OutlineColor = Color3.new(1, 0, 0)
	
		-- BillboardGui
		local billboard = Instance.new("BillboardGui")
		billboard.Parent = animal
		billboard.Adornee = animal
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.AlwaysOnTop = true
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Parent = billboard
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = animal.Name
		textLabel.TextColor3 = Color3.new(1, 0, 0)
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggleEsp = not toggleEsp
	
		if toggleEsp then
			for _, animal in pairs(workspace.animals:GetChildren()) do
				if animal.Name == "deer" then
					createAnimalEsp(animal)
				end
			end
		else
			for _, animal in pairs(workspace.animals:GetChildren()) do
				if animal.Name == "deer" then
					for _, child in pairs(animal:GetChildren()) do
						if child:IsA("Highlight") or child:IsA("BillboardGui") then
							child:Destroy()
						end
					end
				end
			end
		end
	end)
	
	-- Add ESP automatically when new animals spawn
	workspace.animals.ChildAdded:Connect(function(child)
		if toggleEsp and child.Name == "deer" then
			createAnimalEsp(child)
		end
	end)
end
coroutine.wrap(IHDFA_fake_script)()
local function KWEQKZ_fake_script() -- TextButton_18.LocalScript 
	local script = Instance.new('LocalScript', TextButton_18)

	local button = script.Parent
	local toggleEsp = false
	local localPlayer = game.Players.LocalPlayer
	
	-- Function to create ESP for a single player
	local function createPlayerEsp(player)
		if player == localPlayer then return end
		local char = player.Character
		if not char then return end
		if char:FindFirstChild("Highlight") or char:FindFirstChild("BillboardGui") then return end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Parent = char
		highlight.Adornee = char
		highlight.FillTransparency = 1
		highlight.OutlineTransparency = 0
		highlight.OutlineColor = Color3.new(0, 1, 0) -- green color
	
		-- BillboardGui
		local billboard = Instance.new("BillboardGui")
		billboard.Parent = char
		billboard.Adornee = char
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.AlwaysOnTop = true
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Parent = billboard
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = player.Name
		textLabel.TextColor3 = Color3.new(0, 1, 0) -- green color
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggleEsp = not toggleEsp
	
		for _, player in pairs(game.Players:GetPlayers()) do
			if toggleEsp then
				createPlayerEsp(player)
			else
				local char = player.Character
				if char then
					for _, child in pairs(char:GetChildren()) do
						if child:IsA("Highlight") or child:IsA("BillboardGui") then
							child:Destroy()
						end
					end
				end
			end
		end
	end)
	
	-- Automatically add ESP to new players
	game.Players.PlayerAdded:Connect(function(player)
		if toggleEsp then
			player.CharacterAdded:Connect(function()
				createPlayerEsp(player)
			end)
		end
	end)
end

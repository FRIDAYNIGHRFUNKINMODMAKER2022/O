coroutine.wrap(KTFGCNQ_fake_script)()
local function SBMM_fake_script() -- TextButton_20.LocalScript 
	local script = Instance.new('LocalScript', TextButton_20)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	local button = script.Parent
	
	local toggle = false
	local billboards = {}
	
	-- Function to create a Billboard for a trunk
	local function createBillboard(trunk)
		if not trunk or not trunk.Parent then return end
		local hp = trunk.Parent:FindFirstChild("hp") -- lowercase hp
		if not hp then return end
	
		local billboard = Instance.new("BillboardGui")
		billboard.Parent = trunk
		billboard.Adornee = trunk
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.AlwaysOnTop = true
		billboard.StudsOffset = Vector3.new(0, -2, 0) -- slightly above trunk
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Parent = billboard
		textLabel.Size = UDim2.new(1,0,1,0)
		textLabel.BackgroundTransparency = 1
		textLabel.TextColor3 = Color3.new(0,1,0)
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
	
		-- Update HP every frame
		local conn
		conn = RunService.RenderStepped:Connect(function()
			if not trunk or not trunk.Parent then
				if billboard then billboard:Destroy() end
				if conn then conn:Disconnect() end
				return
			end
			textLabel.Text = "HP: "..math.floor(hp.Value)
		end)
	
		table.insert(billboards, {billboard = billboard, connection = conn})
	end
	
	-- Enable HP for all trees
	local function enableAllTreeHp()
		if workspace:FindFirstChild("trees") then
			for _, tree in pairs(workspace.trees:GetChildren()) do
				local trunk = tree:FindFirstChild("Trunk")
				if trunk then
					createBillboard(trunk)
				end
			end
		end
	end
	
	-- Disable all HP displays
	local function disableAllTreeHp()
		for _, data in pairs(billboards) do
			if data.connection then data.connection:Disconnect() end
			if data.billboard then data.billboard:Destroy() end
		end
		billboards = {}
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggle = not toggle
		if toggle then
			script.Parent.Text = "STOP ESP TREE HP"
			enableAllTreeHp()
		else
			disableAllTreeHp()
			script.Parent.Text = "ESP TREE HP"
		end
	end)
	
	-- Handle new trees dynamically
	workspace.trees.ChildAdded:Connect(function(tree)
		if toggle then
			local trunk = tree:FindFirstChild("Trunk")
			if trunk then
				createBillboard(trunk)
			end
		end
	end)
end
coroutine.wrap(SBMM_fake_script)()
local function GUFURU_fake_script() -- TextButton_21.LocalScript 
	local script = Instance.new('LocalScript', TextButton_21)

	local button = script.Parent -- the TextButton
	local runLoop = false
	
	local function createBillboard(parent, growthValue)
		local billboard = parent:FindFirstChild("GrowthBillboard")
		if not billboard then
			billboard = Instance.new("BillboardGui")
			billboard.Name = "GrowthBillboard"
			billboard.Size = UDim2.new(0, 100, 0, 50)
			billboard.Adornee = parent
			billboard.AlwaysOnTop = true
			billboard.Parent = parent
	
			local textLabel = Instance.new("TextLabel")
			textLabel.Name = "GrowthLabel"
			textLabel.Size = UDim2.new(1, 0, 1, 0)
			textLabel.BackgroundTransparency = 1
			textLabel.TextColor3 = Color3.new(1,1,1)
			textLabel.TextScaled = true
			textLabel.Parent = billboard
		end
	
		local label = billboard:FindFirstChild("GrowthLabel")
		if label then
			label.Text = "GROW: " .. tostring(growthValue)
		end
	end
	
	-- Function to handle all current and new plants
	local function handlePlant(planted)
		local trunk = planted:FindFirstChild("Trunk")
		local growth = planted:FindFirstChild("growth")
		if trunk and growth then
			createBillboard(trunk, growth.Value)
		end
	end
	
	-- ChildAdded listener
	local plantedFolder = workspace:WaitForChild("planted") -- lowercase!
	plantedFolder.ChildAdded:Connect(function(newPlant)
		if runLoop then
			handlePlant(newPlant)
		end
	end)
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		runLoop = not runLoop
		button.Text = runLoop and "STOP ESP SEED GROW" or "ESP SEED GROW"
	
		if runLoop then
			-- Update existing plants immediately
			for _, planted in pairs(plantedFolder:GetChildren()) do
				handlePlant(planted)
			end
	
			-- Loop to update growth values
			while runLoop do
				for _, planted in pairs(plantedFolder:GetChildren()) do
					local trunk = planted:FindFirstChild("Trunk")
					local growth = planted:FindFirstChild("growth")
					if trunk and growth then
						createBillboard(trunk, growth.Value)
					end
				end
				wait(0.5)
			end
		end
	end)
end
coroutine.wrap(GUFURU_fake_script)()
local function EURP_fake_script() -- TextButton_22.LocalScript 
	local script = Instance.new('LocalScript', TextButton_22)

	local button = script.Parent
	local toggle = false
	
	-- Function to add Billboard and Highlight
	local function addVisuals(item)
		-- Skip if already has visuals
		if item:FindFirstChild("Highlight") or item:FindFirstChild("BillboardGui") then return end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Parent = item
		highlight.Adornee = item
		highlight.FillTransparency = 1
		highlight.OutlineTransparency = 0
		highlight.OutlineColor = Color3.new(1, 0, 0)
	
		-- BillboardGui
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "DroppedBillboard"
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.Adornee = item
		billboard.AlwaysOnTop = true
		billboard.Parent = item
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = item.Name
		textLabel.TextColor3 = Color3.new(1, 0, 0)
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
		textLabel.Parent = billboard
	end
	
	-- Function to handle all current and new dropped items
	local function handleDroppedItem(item)
		if toggle then
			addVisuals(item)
		end
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggle = not toggle
		button.Text = toggle and "STOP ESP DROPPED" or "ESP DROPPED"
	
		if toggle then
			-- Add visuals to existing items
			for _, item in pairs(workspace.dropped:GetChildren()) do
				handleDroppedItem(item)
			end
		else
			-- Remove visuals from all items
			for _, item in pairs(workspace.dropped:GetChildren()) do
				for _, child in pairs(item:GetChildren()) do
					if child:IsA("Highlight") or child:IsA("BillboardGui") then
						child:Destroy()
					end
				end
			end
		end
	end)
	
	-- Automatically add visuals for new items if toggle is on
	workspace.dropped.ChildAdded:Connect(function(child)
		handleDroppedItem(child)
	end)
end
coroutine.wrap(EURP_fake_script)()
local function NYOACI_fake_script() -- TextButton_23.LocalScript 
	local script = Instance.new('LocalScript', TextButton_23)

	local button = script.Parent
	local toggle = false
	
	-- Function to add Billboard and Highlight to a "well"
	local function addWellVisuals(well)
		-- Skip if already has visuals
		if well:FindFirstChild("Highlight") or well:FindFirstChild("BillboardGui") then return end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Parent = well
		highlight.Adornee = well
		highlight.FillTransparency = 1
		highlight.OutlineTransparency = 0
		highlight.OutlineColor = Color3.fromRGB(0, 0, 255) -- blue
	
		-- BillboardGui
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "WellBillboard"
		billboard.Size = UDim2.new(0, 100, 0, 50)
		billboard.Adornee = well
		billboard.AlwaysOnTop = true
		billboard.Parent = well
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Text = well.Name
		textLabel.TextColor3 = Color3.fromRGB(0, 0, 255) -- blue
		textLabel.TextScaled = true
		textLabel.Font = Enum.Font.SourceSansBold
		textLabel.Parent = billboard
	end
	
	-- Function to handle a single "well" if toggle is on
	local function handleWell(well)
		if toggle and well.Name == "well" then
			addWellVisuals(well)
		end
	end
	
	-- Toggle button
	button.MouseButton1Click:Connect(function()
		toggle = not toggle
		button.Text = toggle and "STOP ESP WELL" or "ESP WELL"
	
		if toggle then
			-- Add visuals to existing wells
			for _, obj in pairs(workspace:GetDescendants()) do
				if obj.Name == "well" then
					handleWell(obj)
				end
			end
		else
			-- Remove visuals from all wells
			for _, obj in pairs(workspace:GetDescendants()) do
				if obj.Name == "well" then
					for _, child in pairs(obj:GetChildren()) do
						if child:IsA("Highlight") or child:IsA("BillboardGui") then
							child:Destroy()
						end
					end
				end
			end
		end
	end)
	
	-- Automatically add visuals for new wells if toggle is on
	workspace.DescendantAdded:Connect(function(child)
		handleWell(child)
	end)
end
coroutine.wrap(NYOACI_fake_script)()
local function TUEHDS_fake_script() -- TextButton_24.LocalScript 
	local script = Instance.new('LocalScript', TextButton_24)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	
	local button = script.Parent
	local running = false -- toggle state
	local oldCFrame -- store old position
	
	-- loop speed (seconds)
	local LOOP_DELAY = 1 
	
	local function doHarvestLoop()
		while running do
			for _, obj in pairs(workspace.harvest:GetChildren()) do
				if not running then break end -- instant stop check
	
				-- Only affect objects named "berry"
				if obj.Name == "berry" then
					local main = obj:FindFirstChild("main")
					if main then
						local prompt = main:FindFirstChildWhichIsA("ProximityPrompt")
						if prompt and prompt.Enabled then
							if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
								local hrp = player.Character.HumanoidRootPart
	
								-- Before TP, check again
								if not running then break end
	
								-- Teleport player slightly in front of main
								hrp.CFrame = main.CFrame + Vector3.new(0,1,0)
								task.wait(.2)
	
								-- Fire prompt
								if running then
									fireproximityprompt(prompt)
								else
									break
								end
							end
						end
					end
				end
			end
			task.wait(LOOP_DELAY)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		running = not running
		if running then
			button.Text = "STOP AUTO BERRY"
			-- Save old position
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				oldCFrame = player.Character.HumanoidRootPart.CFrame
			end
			task.spawn(doHarvestLoop)
		else
			button.Text = "AUTO BERRY"
			-- Teleport back instantly
			if oldCFrame and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				player.Character.HumanoidRootPart.CFrame = oldCFrame
			end
		end
	end)
	
end
coroutine.wrap(TUEHDS_fake_script)()

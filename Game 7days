-- Scripts:

local function FZTXZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- Put this LocalScript inside your TextButton
	
	local button = script.Parent
	local player = game.Players.LocalPlayer
	local userInput = game:GetService("UserInputService")
	
	local infJumpEnabled = false
	
	-- Toggle on button click
	button.MouseButton1Click:Connect(function()
		infJumpEnabled = not infJumpEnabled
		if infJumpEnabled then
			button.Text = "STOP INF JUMP"
		else
			button.Text = "INF JUMP"
		end
	end)
	
	-- Infinite Jump logic
	userInput.JumpRequest:Connect(function()
		if infJumpEnabled then
			local char = player.Character
			if char and char:FindFirstChildOfClass("Humanoid") then
				char:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
			end
		end
	end)
end
coroutine.wrap(FZTXZ_fake_script)()
local function OQHX_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local player = game.Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	local textBox = script.Parent.TextBox
	local button = script.Parent
	
	-- Loop toggle variables
	local running = false
	local loopConnection
	local oldSpeed = nil
	
	-- Toggle function
	button.MouseButton1Click:Connect(function()
		local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if not humanoid then return end
	
		if running then
			-- Stop loop
			running = false
			button.Text = "SET"
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
			-- Restore old speed
			if oldSpeed then
				humanoid.WalkSpeed = oldSpeed
			end
		else
			-- Start loop
			running = true
			button.Text = "STOP"
			oldSpeed = humanoid.WalkSpeed -- save the current speed once
			loopConnection = game:GetService("RunService").RenderStepped:Connect(function()
				if humanoid.Parent then
					local speed = tonumber(textBox.Text)
					if speed then
						humanoid.WalkSpeed = speed
					end
				end
			end)
		end
	end)
	
end
coroutine.wrap(OQHX_fake_script)()
local function GATBCV_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local button = script.Parent
	local runService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	local player = game:GetService("Players").LocalPlayer
	
	local running = false
	local loopConnection
	
	button.MouseButton1Click:Connect(function()
		running = not running
	
		if running then
			button.Text = "STOP AIMBOT SCPS"
	
			loopConnection = runService.RenderStepped:Connect(function()
				local nearestHead
				local shortestDistance = math.huge
				local camPos = camera.CFrame.Position
	
				for _, scp in ipairs(workspace:WaitForChild("scps"):GetChildren()) do
					local head = scp:FindFirstChild("Head")
					if head then
						local dist = (head.Position - camPos).Magnitude
						if dist < shortestDistance then
							shortestDistance = dist
							nearestHead = head
						end
					end
				end
	
				if nearestHead then
					camera.CFrame = CFrame.new(camPos, nearestHead.Position)
				end
			end)
	
		else
			button.Text = "AIMBOT SCPS"
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
		end
	end)
end
coroutine.wrap(GATBCV_fake_script)()
local function IXZIRS_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	-- Variables
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local runService = game:GetService("RunService")
	local button = script.Parent
	
	-- Toggle state
	local noclipEnabled = false
	local connection
	
	-- Function to start noclip loop
	local function startNoclip()
		connection = runService.Stepped:Connect(function()
			if character and character:FindFirstChild("HumanoidRootPart") then
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			end
		end)
	end
	
	-- Function to stop noclip loop
	local function stopNoclip()
		if connection then
			connection:Disconnect()
			connection = nil
		end
		-- Reset CanCollide
		for _, part in pairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = true
			end
		end
	end
	
	-- Button click
	button.MouseButton1Click:Connect(function()
		noclipEnabled = not noclipEnabled
		if noclipEnabled then
			startNoclip()
			button.Text = "STOP NOCLIP"
		else
			stopNoclip()
			button.Text = "NOCLIP"
		end
	end)
	
end
coroutine.wrap(IXZIRS_fake_script)()
local function CINO_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	--// GUI Setup
	local button = script.Parent
	local Lighting = game:GetService("Lighting")
	
	button.MouseButton1Click:Connect(function()
		-- Set FullBright instantly
		Lighting.Ambient = Color3.new(1, 1, 1)
		Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
		Lighting.Brightness = 2
	end)
end
coroutine.wrap(CINO_fake_script)()
local function BAWK_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local button = script.Parent
	local runService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	local player = game:GetService("Players").LocalPlayer
	
	local running = false
	local loopConnection
	
	button.MouseButton1Click:Connect(function()
		running = not running
	
		if running then
			button.Text = "STOP AIMBOT ANIMALS"
	
			loopConnection = runService.RenderStepped:Connect(function()
				local nearestmain
				local shortestDistance = math.huge
				local camPos = camera.CFrame.Position
	
				for _, scp in ipairs(workspace:WaitForChild("animals"):GetChildren()) do
					if scp.Name == "deer" then
					local main = scp:FindFirstChild("main")
					if main then
						local dist = (main.Position - camPos).Magnitude
						if dist < shortestDistance then
							shortestDistance = dist
								nearestmain = main
							end
						end
					end
				end
	
				if nearestmain then
					camera.CFrame = CFrame.new(camPos, nearestmain.Position)
				end
			end)
	
		else
			button.Text = "AIMBOT ANIMALS"
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
		end
	end)
end
coroutine.wrap(BAWK_fake_script)()
local function IEJYKVH_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local button = script.Parent
	local runService = game:GetService("RunService")
	local player = game.Players.LocalPlayer
	local toggled = false
	local DISTANCE = 8 -- distance check in studs
	
	button.MouseButton1Click:Connect(function()
		toggled = not toggled
		button.Text = toggled and "STOP AURA PICK UP" or "AURA PICK UP"
	end)
	
	runService.Heartbeat:Connect(function()
		if not toggled then return end
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		if not hrp then return end
	
		local harvestObjects = workspace:WaitForChild("harvest"):GetChildren()
		local droppedObjects = workspace:WaitForChild("dropped"):GetChildren()
	
		-- If no objects, wait a bit and skip this loop
		if #harvestObjects == 0 and #droppedObjects == 0 then
			wait(0.5)
			return
		end
	
		-- Harvest objects
		for _, obj in pairs(harvestObjects) do
			local mainPart = obj:FindFirstChild("main")
			if mainPart then
				local prompt = mainPart:FindFirstChildOfClass("ProximityPrompt")
				if prompt and (hrp.Position - mainPart.Position).Magnitude <= DISTANCE then
					fireproximityprompt(prompt)
				end
			end
		end
	
		-- Dropped objects
		for _, obj in pairs(droppedObjects) do
			local handle = obj:FindFirstChild("Handle") or obj:FindFirstChild("main")
			if handle then
				local prompt = handle:FindFirstChildOfClass("ProximityPrompt")
				if prompt and (hrp.Position - handle.Position).Magnitude <= DISTANCE then
					fireproximityprompt(prompt)
				end
			end
		end
	end)
end
